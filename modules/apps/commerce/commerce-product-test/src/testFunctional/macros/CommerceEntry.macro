	macro viewDiscountsEntry {
		AssertTextEquals(
			locator1 = "TextInput#NAME",
			value1 = "${discountName}");

		AssertChecked.assertCheckedNotVisible(locator1 = "CommerceEntry#ACTIVE_CHECKBOX");

		AssertSelectedLabel(
