{
	"dependencies": {
		"clay-button": "2.2.0",
		"clay-icon": "2.2.0",
		"metal": "^2.13.2",
		"metal-component": "^2.16.3",
		"metal-soy": "^2.13.2",
		"metal-state": "^2.13.2"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-preset-env": "^1.6.1",
		"check-source-formatting": "^2.0.4",
		"cross-env": "^5.1.3",
		"liferay-npm-bundler": "^2.1.0",
		"liferay-npm-bundler-preset-liferay-dev": "^1.1.0",
		"metal-tools-soy": "^4.0.0",
		"rimraf": "^2.6.2"
	},
	"name": "document-library-preview-pdf",
	"scripts": {
		"build": "metalsoy --soyDeps \"node_modules/+(clay-*|com.liferay.frontend.js.web)/**/*.soy\" && cross-env NODE_ENV=production babel --source-maps -d classes/META-INF/resources src/main/resources/META-INF/resources && liferay-npm-bundler && npm run cleanSoy",
		"checkFormat": "npm run csf",
		"cleanSoy": "rimraf src/**/*.soy.js",
		"csf": "csf src/**/*.js src/**/*.soy",
		"format": "npm run csf -- -i"
	},
	"version": "1.0.0"
}
