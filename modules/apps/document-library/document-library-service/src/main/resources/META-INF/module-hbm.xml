<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.liferay.document.library.model.DLFileVersionPreview" />
	<import class="com.liferay.document.library.model.DLStorageQuota" />
	<class dynamic-update="true" name="com.liferay.document.library.model.impl.DLFileVersionPreviewImpl" table="DLFileVersionPreview">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="dlFileVersionPreviewId" type="long">
			<generator class="assigned" />
		</id>
		<version access="com.liferay.portal.dao.orm.hibernate.PrivatePropertyAccessor" name="mvccVersion" type="long" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="ctCollectionId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="fileEntryId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="fileVersionId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="previewStatus" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class dynamic-update="true" name="com.liferay.document.library.model.impl.DLStorageQuotaImpl" table="DLStorageQuota">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="dlStorageQuotaId" type="long">
			<generator class="assigned" />
		</id>
		<version access="com.liferay.portal.dao.orm.hibernate.PrivatePropertyAccessor" name="mvccVersion" type="long" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="storageSize" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>