{
	"csfConfig": {
		"path:**/test/**/*.es.js": {
			"js": {
				"lint": {
					"env": {
						"jest": true
					}
				}
			}
		}
	},
	"dependencies": {
		"clay-button": "2.16.0",
		"clay-icon": "2.16.0",
		"metal": "2.16.8",
		"metal-component": "2.16.8",
		"metal-debounce": "^2.0.2",
		"metal-soy": "2.16.8",
		"metal-state": "2.16.8"
	},
	"jest": {
		"globals": {
			"Liferay": {}
		},
		"moduleNameMapper": {
			"(.*)\\.soy": "$1.soy.js"
		},
		"setupFilesAfterEnv": [
			"<rootDir>/test/setup/index.js"
		],
		"snapshotSerializers": [
			"metal-jest-serializer"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/test/setup/",
			"<rootDir>/node_modules/"
		],
		"transformIgnorePatterns": [
			"<rootDir>.*(node_modules)(?!.*metal.*).*$"
		]
	},
	"name": "document-library-preview-image",
	"private": true,
	"scripts": {
		"build": "liferay-npm-scripts build",
		"checkFormat": "liferay-npm-scripts lint",
		"format": "liferay-npm-scripts format",
		"test": "liferay-npm-scripts test"
	},
	"version": "2.0.0"
}
