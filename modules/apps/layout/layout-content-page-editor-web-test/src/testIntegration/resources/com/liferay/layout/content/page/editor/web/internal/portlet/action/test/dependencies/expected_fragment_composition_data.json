									"value": "${MAPPED_TEXT_DEFAULT_VALUE}"
								},
								"mapping": {
									"fieldKey": "title",
									"itemReference": {
										"className": "${MAPPED_TEXT_CLASS_NAME}",
										"classPK": "${MAPPED_TEXT_CLASS_PK}"
									}
								}
							}
						}
