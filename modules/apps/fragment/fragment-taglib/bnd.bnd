Export-Package: com.liferay.fragment.taglib.servlet.taglib
Provide-Capability:\
	osgi.extender;\
		osgi.extender="jsp.taglib";\
		uri="http://liferay.com/tld/fragment";\
		version:Version="${Bundle-Version}"
Web-ContextPath: /fragment-taglib