		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>
		<finder name="SegmentsEntryId" return-type="Collection">
			<finder-column name="segmentsEntryId" />
		</finder>
		<finder name="G_S" return-type="SegmentsExperience" unique="true">
			<finder-column name="groupId" />
			<finder-column name="segmentsExperienceKey" />
		</finder>
		<finder name="G_C_C" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>
		<finder name="G_S_C_C" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="segmentsEntryId" />
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>
		<finder name="G_C_C_P" return-type="SegmentsExperience" unique="true">
			<finder-column name="groupId" />
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
			<finder-column name="priority" />
		</finder>
		<finder name="G_C_C_GtP" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
			<finder-column comparator="&gt;" name="priority" />
		</finder>
		<finder name="G_C_C_A" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
			<finder-column name="active" />
		</finder>
		<finder name="G_S_C_C_A" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column arrayable-operator="OR" name="segmentsEntryId" />
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
			<finder-column name="active" />
		</finder>

		<!-- References -->

		<reference entity="Layout" package-path="com.liferay.portal" />
	</entity>
