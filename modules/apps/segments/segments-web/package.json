{
	"csfConfig": {
		"js": {
			"lint": {
				"env": {
					"browser": true,
					"es6": true,
					"jest": true,
					"node": true
				},
				"extends": "liferay",
				"parser": "babel-eslint",
				"parserOptions": {
					"ecmaVersion": 7
				},
				"plugins": [
					"./node_modules/eslint-plugin-react"
				],
				"rules": {
					"react/jsx-handler-names": [
						2,
						{
							"eventHandlerPrefix": "_handle"
						}
					]
				}
			}
		}
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"date-fns": "^1.30.1",
		"formik": "^1.4.1",
		"metal": "2.16.8",
		"metal-debounce": "2.0.1",
		"prop-types": "^15.6.2",
		"react": "^16.6.3",
		"react-dnd": "^7.0.2",
		"react-dnd-html5-backend": "^7.0.2",
		"react-dom": "^16.6.3"
	},
	"description": "",
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/plugin-proposal-class-properties": "^7.4.0",
		"@babel/preset-env": "^7.4.2",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.5",
		"babel-plugin-module-resolver": "^3.2.0",
		"check-source-formatting": "^2.0.14",
		"clay-css": "2.14.2",
		"css-loader": "^2.0.1",
		"eslint": "^5.8.0",
		"eslint-config-liferay": "^2.0.18",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-react": "^7.11.1",
		"eslint-plugin-sort-destructure-keys": "^1.1.0",
		"html-webpack-plugin": "^3.2.0",
		"html-webpack-template": "^6.2.0",
		"jest-dom": "^3.0.0",
		"liferay-lang-key-dev-loader": "^1.0.3",
		"node-sass": "^4.11.0",
		"odata-v4-parser": "^0.1.29",
		"react-testing-library": "^5.3.2",
		"sass-loader": "^7.1.0",
		"snapshot-diff": "^0.4.1",
		"style-loader": "^0.23.1"
	},
	"jest": {
		"moduleNameMapper": {
			"libs/odata-parser": "<rootDir>/src/main/resources/META-INF/resources/js/libs/ODataParser.es.js"
		},
		"setupFilesAfterEnv": [
			"<rootDir>/test/js/setup.js"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/test/js/[^/]+\\.js",
			"<rootDir>/node_modules/"
		]
	},
	"main": "js/index.es.js",
	"name": "segments-web",
	"private": true,
	"scripts": {
		"build": "liferay-npm-scripts build",
		"csf": "csf {src,test}/**/*.es.js src/**/*.scss",
		"format": "npm run csf -- -i",
		"test": "env TZ=utc liferay-npm-scripts test",
		"test:watch": "env TZ=utc liferay-npm-scripts test --watch",
		"watch": "liferay-npm-scripts webpack --watch"
	},
	"version": "2.0.0"
}
