		AppBuilderAdmin.validateItemNameOnTable(itemName = "${formViewName}");

		AppBuilderAdmin.openItem(itemName = "${formViewName}");

		FormViewBuilder.validateFieldLabel(fieldLabel = "${fieldType}");

		AppBuilderAdmin.gotoBack();
	}

