<html>
<head>
<title>AppBuilder</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">AppBuilder</td></tr>
</thead>
<tbody>

<!--CUSTOM_OBJECTS_LISTING_SCREEN-->
<tr>
	<td>BUTTON_ADD</td>
	<td>//button[contains(@class,'btn-primary')]</td>
	<td></td>
</tr>

<tr>
	<td>BUTTON_ADD_NEW_ITEM</td>
	<td>//div[contains(@class,'taglib-empty-result-message')]//button[@class='btn btn-secondary' and contains(text(),'${value1}')]</td>
	<td></td>
</tr>


<tr>
	<td>ITEM_ON_THE_TABLE</td>
	<td>//table[contains(@class,'table')]//*[contains(text(),'${value1}')]</td>
	<td></td>
</tr>

<tr>
	<td>ITEM_ON_THE_TABLE_CLICK</td>
	<td>//table[contains(@class,'table')]//*[contains(text(),'${key_itemName}')]</td>
	<td></td>
</tr>

<tr>
	<td>CHECKBOX</td>
	<td>//input[@class='custom-control-input']</td>
	<td></td>
</tr>
<tr>
	<td>CHECKBOX_OPTION_LABEL</td>
	<td>//label[contains(.,'${key_checkboxOption}')]/input[@type='checkbox']/../span</td>
	<td></td>
</tr>
<tr>
	<td>VERTICAL_ELLIPSIS_BUTTON</td>
	<td>//tr[contains(@data-testid,'item')][descendant::a[contains(.,'${key_itemName}')]]//button[contains(@class,'dropdown-toggle')]</td>
	<td></td>
</tr>

<!--CUSTOM_OBJECTS_LISTING_SCREEN_MODAL_ITEMS-->
<tr>
	<td>INPUT_OBJECT_NAME</td>
	<td>//input[@id='customObjectNameInput']</td>
	<td></td>
</tr>

<tr>
	<td>BUTTON_CONTINUE</td>
	<td>//div[contains(@class,'popover')]//button[contains(@class,'btn-primary')]</td>
	<td></td>
</tr>

<tr>
	<td>BUTTON_CANCEL</td>
	<td>//button[contains(@class,'btn-secondary') and contains(text(),'Cancel')]</td>
	<td></td>
</tr>

<tr>
	<td>CHECKBOX_CONTINUE_AND_CREATE_FORM_VIEW</td>
	<td>//div[contains(@class,'popover')]//input[@class='custom-control-input']</td>
	<td></td>
</tr>

<tr>
	<td>MESSAGE_EXCLAMATION_REQUIRED_FIELD</td>
	<td>//strong[contains(text(),'${value1}')]</td>
	<td></td>
</tr>

<tr>
	<td>MESSAGE_TITLE_EMPTY_LIST</td>
	<td>//h1[contains(@class,'taglib-empty-result-message-title') and contains(text(),'${value1}')]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_DESCRIPTION_EMPTY_LIST</td>
	<td>//p[contains(@class,'taglib-empty-result-message-description') and contains (text(),'${value1}')]</td>
	<td></td>
</tr>



<!--FORM_VIEW_LISTING_SCREEN-->
<tr>
	<td>CUSTOM_OBJECT_NAME_ON_HEADER</td>
	<td>//span[contains(@class,'control-menu-level-1-heading') and contains(text(),'${value1}')]</td>
	<td></td>
</tr>

<!--CUSTOM_OBJECT_SCREEN-->
<tr>
	<td>CUSTOM_OBJECT_TABS</td>
	<td>//a[contains(text(),'${value1}')]</td>
	<td></td>
</tr>

<!--EMPTY_FORM_TABLE_VIEW-->
<tr>
	<td>EMPTY_DROP_ZONE</td>
	<td>//div[(@class='empty-drop-zone') or contains(@class,'ddm-empty-page')]</td>
	<td></td>
</tr>

<tr>
	<td>SAVE_BUTTON</td>
	<td>//button[contains(@class,'btn-primary') and contains(text(),'Save')]</td>
	<td></td>
</tr>

<tr>
	<td>FORM_VIEW_TAB</td>
	<td>//div[contains(@class,'navbar')]//a[contains(@class,'active') and normalize-space()='Form Views']</td>
	<td></td>
</tr>

<!--APP_CREATION_SCREEN-->
<tr>
	<td>APP_NAME</td>
	<td>//a[contains(text(),'${key_itemName}')]</td>
	<td></td>
</tr>

<tr>
	<td>DEPLOY_APP_TOGGLE</td>
	<td>//div[contains(@class,'autofit-row')][descendant::h3[contains(.,'${key_deployOption}')]]//input[contains(@class,'toggle-switch-check')]</td>
	<td></td>
</tr>

<tr>
	<td>UNDEPLOY_APP_BUTTON</td>
	<td>//button[@class='btn btn-primary']</td>
	<td></td>
</tr>

<tr>
	<td>UNDEPLOY_APP_STATUS</td>
	<td>//span[@class='label-item label-item-expand']</td>
	<td></td>
</tr>

<tr>
	<td>DEPLOYED_AS</td>
	<td>//td[contains(text(),'${value1}')]</td>
	<td></td>
</tr>

<tr>
	<td>PRODUCT_MENU_DEPLOYED</td>
	<td>//span[@class='control-menu-level-1-heading text-truncate']</td>
	<td></td>
</tr>

<tr>
	<td>STANDALONE_DEPLOYED</td>
	<td>//h1[contains(@class,'app-builder-standalone-name')]</td>
	<td></td>
</tr>

<tr>
	<td>PORTLET_UNDEPLOYED</td>
	<td>//div[@class='alert alert-info']</td>
	<td></td>
</tr>

<tr>
	<td>WIDGET_DEPLOYED</td>
	<td>//*[contains(@class,'portlet-title-text')]</td>
	<td></td>
</tr>

<tr>
	<td>DEPLOY_APP_STATUS</td>
	<td>//span[contains(text(),'Deployed')]</td>
	<td></td>
</tr>

<tr>
	<td>UNDEPLOY_APP_BUTTON</td>
	<td>//button[@class='btn btn-primary']</td>
	<td></td>
</tr>

<tr>
	<td>PRODUCT_MENU_DEPLOYED</td>
<td>//h1[@class='taglib-empty-result-message-title']</td>
	<td></td>
</tr>

<tr>
	<td>STANDALONE_DEPLOYED</td>
<td>//span[@class='control-menu-level-1-heading text-truncate']</td>
	<td></td>
</tr>

<tr>
	<td>PORTLET_UNDEPLOYED</td>
<td>//div[@class='alert alert-info']</td>
	<td></td>
</tr>

<tr>
	<td>WIDGET_DEPLOYED</td>
<td>//h1[@class='taglib-empty-result-message-title']</td>
	<td></td>
</tr>

<!--CONTROL_PANEL-->
<tr>
	<td>CUSTOM_PORTLET</td>
	<td>//div[contains(@class,'panel-app-root')]//a[contains(@data-title,'${key_portlet}')]</td>
	<td></td>
</tr>

<!-- SEARCH_COMMANDS -->
<tr>
	<td>SEARCH_BAR_INPUT</td>
	<td>//input[contains(@class, 'input-group')]</td>
	<td></td>
</tr>

<tr>
	<td>SEARCH_BUTTON</td>
	<td>//*[contains(@class,'lexicon-icon-search')]</td>
	<td></td>
</tr>

<tr>
	<td>CLEAR_SEARCH</td>
	<td>//button[contains(text(),'Clear')]</td>
	<td></td>
</tr>

<tr>
	<td>VALIDATE_SEARCH_RESULTS_BAR</td>
	<td>//div[contains(.,'Results')]//li[@class='tbar-item tbar-item-expand']</td>
	<td></td>
</tr>

<tr>
	<td>VALIDATE_IF_ITEM_IS_NOT_FOUND</td>
	<td>//h1[contains(@class,'taglib-empty-result-message-title')]</td>
	<td></td>
</tr>

<tr>
	<td>VALIDATE_LIST_TITLE_NAME_AT_ROW</td>
	<td>//tr[@data-testid='item'][${key_RowNumber}]//a</td>
	<td></td>
</tr>

<tr>
	<td>SEARCH_FILTER_AND_ORDER_MENU</td>
	<td>//span[contains(text(), 'Filter and Order')]</td>
	<td></td>
</tr>

<tr>
	<td>CHOOSE_SEARCH_FILTER_BY_OPTION</td>
	<td>//button[contains(text(),'${key_FilterOption}')]</td>
	<td></td>
</tr>

<tr>
	<td>FILTER_LIST_ASCEND_AND_DESCEND_BUTTON</td>
	<td>//*[contains(@class,'lexicon-icon-order-arrow')]</td>
	<td></td>
</tr>

 <tr>
 	<td>ENTRY_SUCCESS_MESSAGE</td>
 	<td>//div[contains(text(),'An entry was added.')]</td>
 	<td></td>
 </tr>
 
</tbody>
</table>
</body>
</html>