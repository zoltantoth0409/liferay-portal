{
	"csfConfig": {
		"path:**/test/**/*.es.js": {
			"js": {
				"lint": {
					"env": {
						"jest": true
					},
					"rules": {
						"no-console": 0,
						"no-eval": 0,
						"no-unused-vars": 0
					}
				}
			}
		}
	},
	"dependencies": {
		"clay": "2.14.0",
		"clay-alert": "2.14.0",
		"liferay-amd-loader": "4.0.0-alpha.5",
		"lodash.escape": "4.0.1",
		"lodash.groupby": "4.6.0",
		"lodash.isequal": "4.5.0",
		"lodash.memoize": "4.1.2",
		"lodash.unescape": "4.0.1",
		"metal": "^2.16.2",
		"metal-anim": "^2.0.0",
		"metal-component": "^2.5.13",
		"metal-dom": "^2.5.13",
		"metal-drag-drop": "3.3.0",
		"metal-events": "^2.5.16",
		"metal-keyboard-focus": "^2.0.1",
		"metal-position": "^2.1.1",
		"metal-promise": "^3.0.4",
		"metal-soy": "^2.16.4",
		"metal-uri": "^3.1.2",
		"svg4everybody": "^2.1.8",
		"uuid": "^3.3.2"
	},
	"devDependencies": {
		"babel-loader": "^8.0.5",
		"webpack": "4.29.5",
		"webpack-cli": "^3.2.3",
		"webpack-dev-server": "^3.1.3",
		"webpack-merge": "^4.1.2"
	},
	"jest": {
		"globals": {
			"Liferay": {}
		},
		"setupFiles": [
			"<rootDir>/test/liferay/portlet/mock/setup.es.js"
		],
		"testMatch": [
			"**/test/**/*.js"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules/",
			"<rootDir>/test/liferay/portlet/mock/portlet_data.es.js",
			"<rootDir>/test/liferay/portlet/mock/setup.es.js"
		],
		"testResultsProcessor": "liferay-jest-junit-reporter",
		"testURL": "http://localhost",
		"transformIgnorePatterns": [
			"<rootDir>.*(node_modules)(?!.*metal.*).*$"
		]
	},
	"name": "frontend-js-web",
	"scripts": {
		"build": "liferay-npm-scripts build",
		"checkFormat": "liferay-npm-scripts lint",
		"format": "liferay-npm-scripts format",
		"start": "webpack-dev-server --config webpack.dev.js",
		"test": "liferay-npm-scripts test",
		"webpack": "webpack --config webpack.prod.js",
		"webpack:dev": "webpack --config webpack.dev.js"
	},
	"version": "3.0.0"
}
