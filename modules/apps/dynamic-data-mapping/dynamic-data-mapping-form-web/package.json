{
	"csfConfig": {
		"path:**/*.js": {
			"js": {
				"lint": {
					"env": {
						"browser": true,
						"jest": true
					},
					"extends": "liferay",
					"parser": "babel-eslint",
					"parserOptions": {
						"ecmaVersion": 7
					},
					"rules": {
						"no-unknown-property": [
							2,
							{
								"ignore": [
									"class"
								]
							}
						]
					}
				}
			}
		},
		"path:**/*.jsp": {
			"js": {
				"lint": {
					"env": {
						"browser": true,
						"jest": true,
						"mocha": true
					},
					"extends": "liferay",
					"globals": {
						"$": true,
						"MODULE_PATH": true,
						"ShareFormPopover": true
					}
				}
			}
		}
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"clay-alert": "2.16.0",
		"clay-checkbox": "2.16.0",
		"clay-icon": "2.16.0",
		"clay-management-toolbar": "2.16.0",
		"clay-modal": "2.16.0",
		"clay-radio": "2.16.0",
		"metal-clipboard": "2.0.1",
		"metal-debounce": "^2.0.2",
		"metal-dom": "2.16.8",
		"metal-drag-drop": "3.3.1",
		"metal-events": "2.16.8",
		"metal-jsx": "2.16.8",
		"metal-position": "2.1.2",
		"metal-state": "2.16.8",
		"object-hash": "^1.3.0"
	},
	"jest": {
		"moduleNameMapper": {
			"(.*)\\.soy$": "$1.soy.js",
			"dynamic-data-mapping-form-builder/(.*)": "<rootDir>/../dynamic-data-mapping-form-builder/src/main/resources/META-INF/resources/$1",
			"source/(.*)": "<rootDir>/src/main/resources/META-INF/resources/admin/js/$1.js"
		},
		"modulePathIgnorePatterns": [
			"/build/",
			"/classes/",
			"/__fixtures__/",
			"/__mock__/"
		],
		"setupFiles": [
			"<rootDir>/jest-setup.config.js"
		],
		"setupFilesAfterEnv": [
			"<rootDir>jest-setup-after-env.config.js"
		],
		"snapshotSerializers": [
			"metal-jest-serializer"
		],
		"testPathIgnorePatterns": [
			"ShareFormPopover"
		],
		"transform": {
			".+": "./jest-workspaces-transform.js"
		}
	},
	"main": "admin/js/main.es.js",
	"name": "dynamic-data-mapping-form-web",
	"private": true,
	"scripts": {
		"build": "liferay-npm-scripts build",
		"checkFormat": "liferay-npm-scripts lint",
		"format": "liferay-npm-scripts format",
		"test": "liferay-npm-scripts test"
	},
	"version": "3.0.0"
}