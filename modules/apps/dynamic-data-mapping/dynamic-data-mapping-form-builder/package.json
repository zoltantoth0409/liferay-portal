{
	"csfConfig": {
		"path:**/*.js": {
			"js": {
				"lint": {
					"env": {
						"browser": true,
						"jest": true
					},
					"extends": "liferay",
					"parser": "babel-eslint",
					"parserOptions": {
						"ecmaVersion": 7
					},
					"rules": {
						"no-unknown-property": [
							2,
							{
								"ignore": [
									"class",
									"xlink:href"
								]
							}
						]
					}
				}
			}
		}
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"clay-alert": "2.15.1",
		"clay-checkbox": "2.15.1",
		"clay-dropdown": "2.15.1",
		"clay-icon": "2.15.1",
		"clay-management-toolbar": "2.15.1",
		"clay-modal": "2.15.1",
		"clay-radio": "2.15.1",
		"metal-clipboard": "2.0.1",
		"metal-debounce": "2.0.1",
		"metal-dom": "2.16.8",
		"metal-drag-drop": "3.3.1",
		"metal-events": "2.16.8",
		"metal-jsx": "2.16.8",
		"metal-position": "2.1.2",
		"metal-state": "2.16.8",
		"metal-useragent": "3.0.1",
		"object-hash": "^1.3.0"
	},
	"jest": {
		"automock": false,
		"coverageDirectory": "build/coverage",
		"coverageThreshold": {
			"global": {
				"functions": 80,
				"lines": 80
			}
		},
		"moduleNameMapper": {
			"(.*)\\.soy$": "$1.soy.js",
			"fixtures/(.*)": "<rootDir>/test/metal/js/__fixtures__/$1.js",
			"mock/(.*)": "<rootDir>/test/metal/js/__mock__/$1.js",
			"source/(.*)": "<rootDir>/src/main/resources/META-INF/resources/js/$1.js"
		},
		"modulePathIgnorePatterns": [
			"/__fixtures__/",
			"/__mock__/",
			"/classes/"
		],
		"setupFiles": [
			"<rootDir>/test/metal/setup/setup.js"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/test/metal/setup/setup_framework.js"
		],
		"snapshotSerializers": [
			"metal-jest-serializer"
		],
		"testMatch": [
			"<rootDir>/test/metal/js/**/*.js"
		],
		"testResultsProcessor": "liferay-jest-junit-reporter",
		"testURL": "http://localhost",
		"transformIgnorePatterns": [
			"<rootDir>.*(node_modules)(?!.*metal.*).*$"
		]
	},
	"name": "dynamic-data-mapping-form-builder",
	"scripts": {
		"build": "liferay-npm-scripts build",
		"checkFormat": "liferay-npm-scripts lint",
		"format": "liferay-npm-scripts format"
	},
	"version": "3.0.0"
}
