{
	"csfConfig": {
		"path:**/src/main/resources/META-INF/resources/*/*.js": {
			"js": {
				"lint": {
					"env": {
						"browser": true
					},
					"extends": "liferay",
					"globals": {
						"AlloyEditor": true,
						"DDMDate": true,
						"DDMNumeric": true,
						"DDMSelect": true,
						"MODULE_PATH": true
					},
					"parser": "babel-eslint",
					"parserOptions": {
						"ecmaVersion": 5
					}
				}
			}
		},
		"path:**/src/main/resources/META-INF/resources/config.js": {
			"js": {
				"lint": {
					"env": {
						"browser": true
					},
					"extends": "liferay",
					"globals": {
						"AlloyEditor": true,
						"MODULE_PATH": true
					},
					"parser": "babel-eslint",
					"parserOptions": {
						"ecmaVersion": 5
					}
				}
			}
		},
		"path:**/src/main/resources/META-INF/resources/metal/**/*.js": {
			"js": {
				"lint": {
					"env": {
						"browser": true,
						"jest": true
					},
					"extends": "liferay",
					"globals": {
						"AlloyEditor": true
					},
					"parser": "babel-eslint",
					"parserOptions": {
						"ecmaVersion": 7
					},
					"plugins": [
						"eslint-plugin-react",
						"sort-class-members"
					],
					"rules": {
						"react/jsx-handler-names": [
							2,
							{
								"eventHandlerPrefix": "_handle"
							}
						],
						"sort-class-members/sort-class-members": [
							2,
							{
								"groups": {
									"conventional-private-methods": [
										{
											"name": "/^_.+$/",
											"sort": "alphabetical",
											"type": "method"
										}
									],
									"methods": [
										{
											"sort": "alphabetical",
											"type": "method"
										}
									]
								},
								"order": [
									"[static-properties]",
									"[static-methods]",
									"[properties]",
									"[conventional-private-properties]",
									"constructor",
									"[methods]",
									"[conventional-private-methods]"
								]
							}
						],
						"sort-keys": [
							"error",
							"asc",
							{
								"caseSensitive": true,
								"natural": false
							}
						]
					}
				}
			}
		}
	},
	"dependencies": {
		"clay-button": "2.14.2",
		"clay-checkbox": "2.14.2",
		"clay-dropdown": "2.14.2",
		"clay-icon": "2.14.2",
		"clay-label": "2.14.2",
		"clay-radio": "2.14.2",
		"metal-component": "2.16.8",
		"metal-drag-drop": "3.3.0",
		"metal-position": "2.1.2",
		"metal-soy": "2.16.8",
		"moment": "^2.22.2",
		"text-mask-addons": "^3.7.1",
		"vanilla-text-mask": "^5.0.1"
	},
	"jest": {
		"moduleNameMapper": {
			"(.*)\\.soy$": "$1.soy.js",
			"dynamic-data-mapping-form-builder/(.*)": "<rootDir>/../dynamic-data-mapping-form-builder/src/main/resources/META-INF/resources/$1",
			"source/(.*)": "<rootDir>/src/main/resources/META-INF/resources/metal/$1.js"
		},
		"modulePathIgnorePatterns": [
			"/__fixtures__/",
			"/__mock__/",
			"/classes/"
		],
		"setupFiles": [
			"<rootDir>/jest-setup.config.js"
		],
		"snapshotSerializers": [
			"metal-jest-serializer"
		],
		"testPathIgnorePatterns": [
			"DatePicker",
			"Numeric",
			"Options",
			"Select",
			"Tooltip",
			"Validation"
		],
		"transform": {
			".+": "./jest-workspaces-transform.js"
		}
	},
	"main": "./",
	"name": "dynamic-data-mapping-form-field-type",
	"private": true,
	"scripts": {
		"build": "liferay-npm-scripts build",
		"checkFormat": "liferay-npm-scripts lint",
		"format": "liferay-npm-scripts format",
		"test": "liferay-npm-scripts test"
	},
	"version": "4.0.0"
}
