		Ratings.viewRateStar(
			averageRatingStarCount = "2.0",
			ratingStarCount = "2");

		Comments.viewPG(
			commentCount = "1",
			entryComment = "Wiki FrontPage Comment",
			userFullName = "Test Test (You)");
