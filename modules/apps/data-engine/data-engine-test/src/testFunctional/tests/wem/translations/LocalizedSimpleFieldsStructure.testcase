	@description = "This is a test for LPS-122278. As Content Creator, I want to define reusable translations when creating WEM Structures without nested fields or fieldsets"
	@priority = "5"
	test TranslationDoesNotChangeAnother {
		property portal.acceptance = "false";

		ProductMenu.gotoPortlet(
			category = "Content & Data",
			portlet = "Web Content");

		NavItem.gotoStructures();

		WebContentStructures.addCP(
			structureDescription = "Test",
			structureName = "Test");

		DataEngine.addField(
			fieldFieldLabel = "Text Test",
			fieldName = "Text");

		DataEngine.editFieldTip(
			fieldFieldLabel = "Text Test",
			fieldHelp = "Help Text test");

		PortletEntry.changeLocale(locale = "pt-BR");

		DataEngine.editFieldTip(
			fieldFieldLabel = "Text Test",
			fieldHelp = "Texto teste ajuda");

		WebContentStructures.addName(structureName = "Teste");

		FormViewBuilder.editFieldLabel(
			fieldLabel = "Text Test",
			label = "Texto Teste");

		PortletEntry.changeLocale(locale = "de-DE");

		DataEngine.editFieldTip(
			fieldFieldLabel = "Text Test",
			fieldHelp = "Text test help German");

		WebContentStructures.addName(structureName = "Test German");

		FormViewBuilder.editFieldLabel(
			fieldLabel = "Text Test",
			label = "Text test German");

		WebContentStructures.saveCP(structureName = "Test");

		WebContentStructures.viewCP(
			structureDescription = "Test",
			structureName = "Test");

		LexiconEntry.gotoEntry(rowEntry = "Test");

		AssertTextEquals(
			key_fieldFieldLabel = "Text Test",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER_LABEL",
			value1 = "Text Test");

		AssertTextEquals(
			key_fieldFieldLabel = "Text Test",
			locator1 = "DDMEditStructure#SETTINGS_TEXT",
			value1 = "Help Text test");

		AssertTextEquals(
			locator1 = "TextInput#NAME",
			value1 = "Test");

		PortletEntry.changeLocale(locale = "pt-BR");

		AssertTextEquals(
			key_fieldFieldLabel = "Texto Teste",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER_LABEL",
			value1 = "Texto Teste");

		AssertTextEquals(
			key_fieldFieldLabel = "Texto Teste",
			locator1 = "DDMEditStructure#SETTINGS_TEXT",
			value1 = "Texto teste ajuda");

		AssertTextEquals(
			locator1 = "TextInput#NAME",
			value1 = "Teste");

		PortletEntry.changeLocale(locale = "de-DE");

		AssertTextEquals(
			key_fieldFieldLabel = "Text test German",
			locator1 = "DDMEditStructure#FORM_FIELD_CONTAINER_LABEL",
			value1 = "Text test German");

		AssertTextEquals(
			key_fieldFieldLabel = "Text test German",
			locator1 = "DDMEditStructure#SETTINGS_TEXT",
			value1 = "Text test help German");

		AssertTextEquals(
			locator1 = "TextInput#NAME",
			value1 = "Test German");

		WebContentStructures.saveCP(structureName = "Test");

		WebContentStructures.deleteCP(structureName = "Test");
	}

