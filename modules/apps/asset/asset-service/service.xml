<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder auto-import-default-references="false" auto-namespace-tables="false" package-path="com.liferay.asset">
	<namespace>Asset</namespace>
	<entity local-service="true" name="AssetEntryUsage" remote-service="false" uuid="true">

		<!-- PK fields -->

		<column name="assetEntryUsageId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="assetEntryId" type="long" />
		<column name="classNameId" type="long" />
		<column name="classPK" type="long" />
		<column name="portletId" type="String" />
		<column name="lastPublishDate" type="Date" />

		<!-- Finder methods -->

		<finder name="AssetEntryId" return-type="Collection">
			<finder-column name="assetEntryId" />
		</finder>
		<finder name="A_C" return-type="Collection">
			<finder-column name="assetEntryId" />
			<finder-column name="classNameId" />
		</finder>
		<finder name="C_C_P" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
			<finder-column name="portletId" />
		</finder>

		<!-- References -->

		<reference entity="User" package-path="com.liferay.portal" />
	</entity>
</service-builder>