		AppBuilderAdmin.addEntry();

		FormViewBuilder.editText(
			fieldName = "Text",
			fieldValue = "Content Test 1");

		AppBuilderAdmin.save();

		AppBuilderAdmin.addEntry();

		FormViewBuilder.editText(
			fieldName = "Text",
			fieldValue = "Content Test 2");

		AppBuilderAdmin.save();

		AppBuilderAdmin.validateItemNameOnTable(itemName = "Content Test 1");
