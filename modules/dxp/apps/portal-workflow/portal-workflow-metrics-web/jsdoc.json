{
	"opts": {
		"destination": "./build/doc/",
		"encoding": "utf8",
		"recurse": true,
		"template": "templates/default"
	},
	"plugins": [
		"plugins/markdown"
	],
	"recurseDepth": 10,
	"source": {
		"excludePattern": "(^|\\/|\\\\)_",
		"include": [
			"src/main/resources/META-INF/resources/js/components",
			"src/main/resources/META-INF/resources/js/shared"
		],
		"includePattern": ".+\\.js(doc|x)?$"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"closure",
			"jsdoc"
		]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}