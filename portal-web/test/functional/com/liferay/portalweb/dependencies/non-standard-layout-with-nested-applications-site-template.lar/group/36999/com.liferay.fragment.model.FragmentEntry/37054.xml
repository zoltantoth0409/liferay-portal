  <__html>&lt;div class=&quot;wb-features&quot;&gt;
	&lt;div class=&quot;wb-secondary-background-color&quot;&gt;
		&lt;div class=&quot;wb-upper-modules&quot;&gt;
			&lt;section class=&quot;container-fluid container-fluid-max-xl&quot;&gt;
				&lt;div class=&quot;row&quot;&gt;
