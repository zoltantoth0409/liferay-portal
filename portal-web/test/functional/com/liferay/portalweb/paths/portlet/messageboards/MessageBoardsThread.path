<html>
<head>
<title>MessageBoardsThread</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">MessageBoardsThread</td></tr>
</thead>
<tbody>
<tr>
	<td>TITLE_HEADER</td>
	<td>//h3[@class='header-title']</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--THREAD-->
<tr>
	<td>THREAD_BODY</td>
	<td>//div[contains(@class,'message-content') and contains(.,'${key_threadBody}')]</td>
	<td></td>
</tr>
<tr>
	<td>THREAD_BODY_2</td>
	<td>xpath=(//div[contains(@class,'message-content') and contains(.,'${key_threadBody}')])[2]</td>
	<td></td>
</tr>
<tr>
	<td>THREAD_REPLY_BUTTON</td>
	<td>//div[@class='thread-container' and contains(.,'${key_threadSubject}')]//span[contains(.,'Reply to Main Thread')]</td>
	<td></td>
</tr>
<tr>
	<td>THREAD_REPLY_SUBJECT</td>
	<td>//div[contains(@class,'list-group-card') and contains(.,'${key_threadBody}')]//h4[contains(.,'${key_threadSubject}')]</td>
	<td></td>
</tr>
<tr>
	<td>THREAD_SUBJECT</td>
	<td>//div[@class='panel-heading' and contains(.,'${key_threadSubject}')]//h4[contains(.,'${key_threadSubject}')]</td>
	<td></td>
</tr>
<tr>
	<td>THREAD_SUBJECT_2</td>
	<td>xpath=(//div[@class='panel-heading' and contains(.,'${key_threadSubject}')]//h4[contains(.,'${key_threadSubject}')])[2]</td>
	<td></td>
</tr>
<tr>
	<td>THREAD_USERNAME</td>
	<td>//div[@class='panel-heading' and contains(.,'${key_threadSubject}')]//h5[contains(.,'${key_userName}')]</td>
	<td></td>
</tr>
<tr>
	<td>THREAD_USERNAME_2</td>
	<td>xpath=(//div[@class='panel-heading' and contains(.,'${key_threadSubject}')]//h5[contains(.,'${key_userName}')])[2]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--THREAD_CONTROLS-->
<tr>
	<td>THREAD_CONTROLS_BACK</td>
	<td>//a[contains(@id,'TabsBack')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--THREAD_ACTIONS-->
<tr>
	<td>THREAD_ACTIONS_UNSUBSCRIBE</td>
	<td>//div[@class='thread-actions']/ul/li/a/span[contains(.,'Unsubscribe')]</td>
	<td></td>
</tr>
<tr>
	<td>THREAD_ACTIONS_UNLOCK_THREAD</td>
	<td>//div[@class='thread-actions']/ul/li/a/span[contains(.,'Unlock Thread')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--MESSAGE-->
<tr>
	<td>MESSAGE_BAN_THIS_USER</td>
	<td>//div[contains(.,'${key_userName}')]/div/span/a/span[contains(.,'Ban This User')]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_WORKFLOW_STATUS_1</td>
	<td>//span[@class='taglib-workflow-status']/span[contains(@class,'workflow-status')]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_BODY_1</td>
	<td>xpath=(//div[@class='thread-body'])[1]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_BODY_STYLE_1</td>
	<td>//div[@class='thread-body']/span[contains(@style,'${key_fontSize}px')]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_USERNAME_2</td>
	<td>xpath=(//span[@class='user-name'])[2]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_ANSWER_1</td>
	<td>//h4[contains(.,'Answer')]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_REPLY_1</td>
	<td>//a/span[contains(.,'Reply to Main Thread')]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_REPLY_SPECIFIC</td>
	<td>//td[contains(.,'${key_threadBody}')]//span[.='Reply']</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_QUICK_REPLY_1</td>
	<td>xpath=(//span[.='Quick Reply'])[1]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_REPLY_WITH_QUOTE_1</td>
	<td>xpath=(//span[.='Reply with Quote'])[1]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_RATING_1</td>
	<td>xpath=(//div[contains(@id,'ratingThumbContent')]/div)[1]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_RATING_2</td>
	<td>xpath=(//div[contains(@id,'ratingThumbContent')]/div)[3]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_RATING_1_UPVOTE</td>
	<td>xpath=(//div[contains(@id,'ratingThumbContent')]/a[contains(@class,'rating-thumb-up')])[1]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_RATING_2_UPVOTE</td>
	<td>xpath=(//div[contains(@id,'ratingThumbContent')]/a[contains(@class,'rating-thumb-up')])[2]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_EDIT_1</td>
	<td>xpath=(//td//ul/li[2]/span/a/span)[1]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_ATTACHMENT_IMAGE</td>
	<td>//div[contains(@class,'message-attachments')]//img[contains(@src,'${key_mbThreadAttachment}')]</td>
	<td></td>
</tr>
<tr>
	<td>MESSAGE_BODY_ATTACHMENT_1</td>
	<td>//li[@class='message-attachment']/span/a</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>