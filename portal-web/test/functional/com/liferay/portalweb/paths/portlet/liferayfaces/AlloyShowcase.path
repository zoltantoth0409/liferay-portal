<html>
<head>
<title>AlloyShowcase</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">AlloyShowcase</td></tr>
</thead>
<tbody>
<tr>
	<td>ALLOY_DATA_LIST_ITEM_DESCRIPTION</td>
	<td>xpath=(//dl[@class='alloy-data-list']//dd)[${key_item}]</td>
	<td></td>
</tr>
<tr>
	<td>ALLOY_DATA_LIST_ITEM_ICON</td>
	<td>//img[contains(@src,'${key_icon}')]</td>
	<td></td>
</tr>
<tr>
	<td>ALLOY_DATA_LIST_ITEM_TERM</td>
	<td>xpath=(//dl[@class='alloy-data-list']//dt)[${key_item}]</td>
	<td></td>
</tr>
<tr>
	<td>DATA_LIST_DESCRIPTIOND</td>
	<td>xpath=(//div[@class='showcase-example']//ol//li)[${key_item}]</td>
	<td></td>
</tr>
<tr>
	<td>DATA_LIST_ORDERED</td>
	<td>xpath=(//div[@class='showcase-example']//ol//li)[${key_item}]</td>
	<td></td>
</tr>
<tr>
	<td>DATA_LIST_UNORDERED</td>
	<td>xpath=(//div[@class='showcase-example']//ul//li)[${key_item}]</td>
	<td></td>
</tr>
<tr>
	<td>DROPDOWN_CARET_BUTTON</td>
	<td>//span[@role='menuitem']//span[contains(@class,'caret')]</td>
	<td></td>
</tr>
<tr>
	<td>DROPDOWN_BUTTON</td>
	<td>//span[contains(@class,'menu-label')]//span[contains(.,'Dropdown')]</td>
	<td></td>
</tr>
<tr>
	<td>EVENT_TABLE</td>
	<td>//td[contains(text(),'${key_eventText}')]</td>
	<td></td>
</tr>
<tr>
	<td>FEED_BACK_MESSAGE</td>
	<td>//table[contains(@class,'alloy-messages')]</td>
	<td></td>
</tr>
<tr>
	<td>FIRST_NAME_CELL</td>
	<td>//tbody//td[1]//span[contains(@class,'alloy-output-text')]</td>
	<td></td>
</tr>
<tr>
	<td>GENERAL_BUTTON_1</td>
	<td>//button[contains(.,'Button')]//img[contains(@src,'liferay-faces-logo-small.png')]/..</td>
	<td></td>
</tr>
<tr>
	<td>GENERAL_BUTTON_2</td>
	<td>//button[not(contains(.,'Button'))]//img[contains(@src,'liferay-faces-logo-small.png')]/..</td>
	<td></td>
</tr>
<tr>
	<td>GENERAL_BUTTON_3</td>
	<td>//label[contains(.,'Example')][contains(.,'value')]/ancestor::div[@class='showcase-example']//button[@type='button' or @value='1234']</td>
	<td></td>
</tr>
<tr>
	<td>MENU_BUTTON</td>
	<td>//span[contains(@class,'menu-label')]//span[contains(@class,'alloy-output-text')]</td>
	<td></td>
</tr>
<tr>
	<td>MENU_BUTTON_2</td>
	<td>xpath=(//span[contains(@class,'menu-label')]//span[contains(@class,'alloy-output-text')])[2]</td>
	<td></td>
</tr>
<tr>
	<td>MENU_ITEM</td>
	<td>//label[contains(.,'Example')][contains(.,'${key_exampleType}')]/ancestor::div[@class='showcase-example']//span[contains(text(),'${key_menuItem}')]</td>
	<td></td>
</tr>
<tr>
	<td>MENU_OUTPUT</td>
	<td>//label[contains(.,'${key_exampleType}')]/parent::div[@class='showcase-example']//ul[contains(@class,'feedback')]/li[contains(@class,'text-info')]</td>
	<td></td>
</tr>
<tr>
	<td>MODAL_DIALOG</td>
	<td>//div[contains(@class,'yui3-widget-modal')]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_BUTTON_ACTIVE</td>
	<td>//div[contains(@class,'alloy-paginator')]//*[contains(@class,'pagination')]//*[contains(.,'${key_pageNum}')]/parent::li[@class='active']</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_BUTTON_DISABLE</td>
	<td>//div[contains(@class,'alloy-paginator')]//*[contains(@class,'pagination')]//li[contains(.,'${key_pageNum}')][contains(@class,'disabled')]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_NUMBER_BUTTON</td>
	<td>//div[contains(@class,'alloy-paginator')]//*[contains(@class,'pagination')]//li/*[contains(.,'${key_pageNum}')]</td>
	<td></td>
</tr>
<tr>
	<td>PAGINATOR_BUTTON</td>
	<td>//div[contains(@class,'alloy-paginator')]//*[contains(@class,'pagination')]//li//*[contains(@onclick,'${key_buttonLabel}')]</td>
	<td></td>
</tr>
<tr>
	<td>PAGINATOR_DROP_DOWN_LIST</td>
	<td>//label[contains(.,'Rows Per Page')]/following-sibling::select/option[@value='${key_opt}']</td>
	<td></td>
</tr>
<tr>
	<td>PAGINATOR_DROP_DOWN_SELECTED</td>
	<td>//select/option[@value='100'][(@selected='selected')]</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_ICON</td>
	<td>xpath=(//*[contains(@class,'icon-search')])[${key_item}]</td>
	<td></td>
</tr>
<tr>
	<td>SELECTED_ROW</td>
	<td>//table[contains(@id,':customers')]//tr[contains(@class,'info')][contains(.,'${key_firstName}')][contains(.,'${key_lastName}')]</td>
	<td></td>
</tr>
<tr>
	<td>SELECTION_CHECKBOX</td>
	<td>//input[@id='_1_WAR_comliferayfacesdemoalloyshowcaseportlet_:exampleForm:customers:${key_num}']</td>
	<td></td>
</tr>
<tr>
	<td>SELECTION_CHECKBOX_ALL</td>
	<td>//input[@id='_1_WAR_comliferayfacesdemoalloyshowcaseportlet_:exampleForm:customers_selectAll']</td>
	<td></td>
</tr>
<tr>
	<td>SELECTION_FIRST_NAME_CELL</td>
	<td>//tbody//td[2]//span[contains(@class,'alloy-output-text')]</td>
	<td></td>
</tr>
<tr>
	<td>SELECTION_MODAL_LINK</td>
	<td>//div[@class='showcase-example']//ul//li[${key_item}]//a</td>
	<td></td>
</tr>
<tr>
	<td>SELECTION_MODAL_SELECT</td>
	<td>//div[contains(.,'Selection Mode')]//option[@value='${key_opt}']</td>
	<td></td>
</tr>
<tr>
	<td>SELECTION_MODAL_TEXT</td>
	<td>//div[@class='yui3-widget-bd modal-body']//div//div//span</td>
	<td></td>
</tr>
<tr>
	<td>SHOW_RESULT_TYPE_BUTTON</td>
	<td>//button[contains(.,'${key_type}')]</td>
	<td></td>
</tr>
<tr>
	<td>TABLE_ARROW_ASCEND</td>
	<td>//th[not (contains(@class,'table-sorted-desc'))]//a[contains(text(),'${key_columnName}')]//span[@class='table-sort-indicator']</td>
	<td></td>
</tr>
<tr>
	<td>TABLE_ARROW_DESCEND</td>
	<td>//th[contains(@class,'table-sorted-desc')]//a[contains(text(),'${key_columnName}')]//span[@class='table-sort-indicator']</td>
	<td></td>
</tr>
<tr>
	<td>TABLE_COLUMN_NAME</td>
	<td>//a[contains(text(),'${key_columnName}')]</td>
	<td></td>
</tr>
<tr>
	<td>TABLE_DATE_OF_BIRTH_ARROW</td>
	<td>//a[contains(text(),'Date of Birth')]//span[@class='table-sort-indicator']</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>