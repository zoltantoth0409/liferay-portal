<html>
<head>
<title>CKEditor</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">CKEditor</td></tr>
</thead>
<tbody>
<tr>
	<td>BODY_FIELD</td>
	<td>//div[contains(@id,'cke')]</td>
	<td></td>
</tr>
<tr>
	<td>BODY_FIELD_SOURCE_ON</td>
	<td>//div[@id='cke_1_contents']/textarea</td>
	<td></td>
</tr>
<tr>
	<td>BODY_FIELD_SOURCE_ON_2</td>
	<td>//div[@id='cke_2_contents']/textarea</td>
	<td></td>
</tr>
<tr>
	<td>BODY_FIELD_IFRAME</td>
	<td>//div[contains(@id,'cke')]//iframe</td>
	<td></td>
</tr>
<tr>
	<td>BODY</td>
	<td>//body[contains(@class,'cke_editable')]</td>
	<td></td>
</tr>
<tr>
	<td>SOURCE_URL</td>
	<td>//input[contains(@id,'sourceURL')]</td>
	<td></td>
</tr>
<tr>
	<td>RESIZER</td>
	<td>//span[@id='cke_1_resizer']</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--TOOLBOX-->
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--TOOLBAR-->
<tr>
	<td>TOOLBAR_SIZE_BUTTON</td>
	<td>//a[contains(@class,'cke_combo_button') and contains(.,'Size')]/span</td>
	<td></td>
</tr>
<tr>
	<td>TOOLBAR_SIZE_BUTTON_DROPDOWN</td>
	<td>//span[contains(@class,'fontsize')]//span[contains(.,'${key_fontSize}')]</td>
	<td></td>
</tr>
<tr>
	<td>TOOLBAR_SOURCE_BUTTON</td>
	<td>//a[contains(@class,'cke_button') and contains(@class,'source')]</td>
	<td></td>
</tr>
<tr>
	<td>TOOLBAR_SOURCE_BUTTON_2</td>
	<td>//div[contains(@class,'cke_2')]//a[contains(@class,'cke_button') and contains(@class,'source')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!-- CONTENT_AUDIO_LINK -->
<tr>
	<td>CONTENT_AUDIO_LINK_IMAGE</td>
	<td>//img[contains(@class,'audio')]</td>
	<td></td>
</tr>
<!--CONTENT_IMAGE_LINK-->
<tr>
	<td>CONTENT_IMAGE_LINK</td>
	<td>//span[@class='cke_toolbox']//span/a[contains(.,'Image')]</td>
	<td></td>
</tr>
<tr>
	<td>CONTENT_IMAGE_LINK_IMAGE</td>
	<td>//img[contains(@src,'jpg')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--CONTENT_VIDEO_LINK-->
<tr>
	<td>CONTENT_VIDEO_LINK_IMAGE</td>
	<td>//img[contains(@class,'video')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>