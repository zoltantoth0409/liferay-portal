<html>
<head>
<title>ManagementBar</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">ManagementBar</td></tr>
</thead>
<tbody>
<tr>
	<td>FILTER_AND_ORDER</td>
	<td>//nav[contains(@class,'management-bar')]//*[*[normalize-space(text())='Filter and Order']]</td>
	<td></td>
</tr>
<tr>
	<td>FILTER_AND_ORDER_ACTIVE_ITEM</td>
	<td>//div[contains(@class,'dropdown-menu')]//a[contains(@class,'active')][contains(text(),'${key_menuItem}')]</td>
	<td></td>
</tr>
<tr>
	<td>FILTER_AND_ORDER_SUBHEADER</td>
	<td>//li[contains(@class,'subheader') and contains(.,'${key_subheaderItem}')]</td>
	<td></td>
</tr>
<tr>
	<td>FILTER_RESULTS_SCOPE</td>
	<td>//span[contains(@class,'label-item')]//div[contains(@class,'label-section') and contains(.,'${key_filterScope}')]</td>
	<td></td>
</tr>
<tr>
	<td>NAVBAR_TEXT</td>
	<td>//nav[contains(@class,'management-bar')]//span[contains(@class,'navbar-text')]</td>
	<td></td>
</tr>
<tr>
	<td>ORDER_ICON_GENERIC</td>
	<td>//nav[contains(@class,'management-bar')]//a[contains(@class,'order') and contains(@class,'active')]</td>
	<td></td>
</tr>
<tr>
	<td>ORDER_ICON_SPECIFIC</td>
	<td>//nav[contains(@class,'management-bar')]//a[contains(@class,'${key_order}')][@title='Reverse Sort Direction']</td>
	<td></td>
</tr>
<tr>
	<td>PORTLET_BODY</td>
	<td>//header[@id='banner']//section[contains(.,'${key_portletName}')]//div[@class='portlet-body']</td>
	<td></td>
</tr>
<tr>
	<td>REMOVE</td>
	<td>//div[contains(@id,'ManagementToolbar')]//button[contains(@title,'Remove')]</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_RESULT_SUMMARY</td>
	<td>//div[contains(@id,'ManagementToolbar')]//span[@class='text-truncate']</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>