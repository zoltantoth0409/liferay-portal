		Click(locator1 = "PageEditor#FRAGMENT_BODY_FLOATING_TOOLBAR_PLUS_BUTTON");

		SelectFrame(locator1 = "IFrame#DIALOG");

		if (isSet(assetListName)) {
			NavItem.click(navItem = "Collections");

