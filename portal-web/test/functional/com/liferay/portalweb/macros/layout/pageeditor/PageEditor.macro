	macro editCollectionDisplay {
		var key_fragmentName = "${fragmentName}";

		Click(locator1 = "PageEditor#CONTENT_PAGE_DISABLED_HEADER");

		Click(locator1 = "Fragment#FRAGMENT_BODY");

