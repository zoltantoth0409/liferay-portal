	macro configureGoogleDrive {
		Type(
			key_text = "clientId",
			locator1 = "TextArea#ANY",
			value1 = "${googleClientID}");

		Type(
			key_text = "clientSecret",
			locator1 = "TextArea#ANY",
			value1 = "${googleClientSecret}");

		if (isSet(googlePickerAPIKey)) {
			Type(
