		);

		AssertTextEquals(
			locator1 = "MenuItem#ANY_MENU_ITEM",
			value1 = "Remove",
			key_menuItem = "Remove"
		);
	}

	macro viewSpecificPG {
		var key_portletTitleName = "${portletName}";

		AssertTextEquals.assertTextCaseInsensitive(
