		);
	}

	@summary = "Assert that the ${portletName} portlet title has the text '${portletName}'"
	macro viewTitle {
		var key_portletName = "${portletName}";

		AssertTextEquals.assertTextCaseInsensitive(
			locator1 = "Portlet#TITLE",
