		SelectFrame(locator1 = "IFrame#SELECT_STRUCTURE_FIELD");

		var key_fieldName = "${fieldFilter}";

		if (isSet(fieldFilter)) {
			Check(locator1 = "APConfigurationFilterbyfield#FIELD_FILTER_RADIO_BUTTON");
		}

		if (isSet(fieldValue)) {
