		ContentTargetingUserSegment.expandCategory();

		var key_userSegmentRule = "${userSegmentRule}";

		DragAndDrop(
			locator1 = "EditUserSegment#USER_SEGMENT_RULES_RULE",
			locator2 = "ContentTargeting#FORM_BUILDER",
			value1 = "${userSegmentRule}");
	}

