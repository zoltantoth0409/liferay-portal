			locator1 = "AlloyShowcase#PAGINATOR_BUTTON");

		AssertElementPresent(
			key_pageNum = "2",
			locator1 = "AlloyShowcase#PAGE_BUTTON_ACTIVE");

		if ("${useCase}" == "selection" || "${useCase}" == "serverEvents") {
