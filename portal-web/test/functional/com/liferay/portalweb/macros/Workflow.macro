<definition>
	<command name="_gotoPendingFilter">
		<execute function="Pause" locator1="5000" />

		<execute function="Click" locator1="Dropdown#FILTER" />

		<var name="key_filterValue" value="Pending" />

		<execute function="AssertClick" locator1="Dropdown#FILTER_VALUES" value1="Pending" />
	</command>

	<command name="_openPreview">
		<if>
			<condition function="IsElementNotPresent#isNotVisible" locator1="MyWorkflowTasksTask#PREVIEW_TITLE" />
			<then>
				<execute function="Click" locator1="MyWorkflowTasksTask#PREVIEW" />
			</then>
		</if>
	</command>

	<command name="_tearDownWorkflow">
		<var name="noWorkflow" value="No Workflow" />

		<if>
			<condition function="IsElementPresent" locator1="Icon#ROW_VERTICAL_ELLIPSIS" />
			<then>
				<execute macro="LexiconEntry#gotoEntryMenuItem">
					<var name="menuItem" value="Edit" />
					<var name="rowEntry" value="Knowledge Base Article" />
				</execute>

				<execute function="Select" locator1="WorkflowConfiguration#RESOURCE_TABLE_SELECT" value1="${noWorkflow}">
					<var name="key_workflowResourceValue" value="Knowledge Base Article" />
				</execute>

				<execute macro="Button#clickSave" />
			</then>
		</if>

		<for list="Blogs Entry,Calendar Event,Comment,Message Boards Message,Page Revision,Wiki Page" param="workflowResourceValue">
			<execute macro="LexiconEntry#gotoEntryMenuItem">
				<var name="menuItem" value="Edit" />
				<var name="rowEntry" value="${workflowResourceValue}" />
			</execute>

			<var name="key_workflowResourceValue" value="${workflowResourceValue}" />

			<execute function="Select" locator1="WorkflowConfiguration#RESOURCE_TABLE_SELECT" value1="${noWorkflow}" />

			<execute macro="Button#clickSave" />
		</for>
	</command>

	<command name="_viewActivities">
		<var name="key_workflowAssetTitle" value="${workflowAssetTitle}" />

		<execute function="AssertClick" locator1="MyWorkflowTasks#ASSIGNED_TO_ME_TABLE_ASSET_TITLE" value1="${workflowAssetTitle}" />

		<execute function="AssertClick" locator1="MyWorkflowTasksTask#ACTIVITIES" value1="Activities" />

		<execute function="AssertTextEquals#assertPartialText" locator1="MyWorkflowTasksTask#ACTIVITIES_LINE_1" value1="${workflowActivityComment}" />
	</command>

	<command name="addNewDefinition">
		<execute macro="LexiconEntry#gotoAdd" />

		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="${workflowDefinitionTitle}" />
		</execute>
	</command>

	<command name="approveTaskByActions">
		<execute macro="Workflow#viewTaskAssignedToMe">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute macro="Workflow#gotoTaskAssignedToMeActionsCmd">
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<if>
			<isset var="workflowDefinitionTask" />
			<then>
				<var name="key_menuListOption" value="${workflowDefinitionTask}" />

				<execute function="AssertClick#pauseAssertTextClickAt" locator1="MyWorkflowTasks#ASSIGNED_TO_ME_MENULIST_OPTION" value1="${workflowDefinitionTask}" />
			</then>
			<elseif>
				<isset var="boardWorkflowTask" />
				<then>
					<execute function="AssertClick#pauseAssertTextClickAt" locator1="MenuItem#ANY_MENU_ITEM" value1="Approve ${boardWorkflowTask}">
						<var name="key_menuItem" value="Approve ${boardWorkflowTask}" />
					</execute>
				</then>
			</elseif>
			<else>
				<execute function="AssertClick#pauseAssertTextClickAt" locator1="MenuItem#ANY_MENU_ITEM" value1="Approve">
					<var name="key_menuItem" value="Approve" />
				</execute>
			</else>
		</if>

		<execute macro="IFrame#selectConfigurationFrame" />

		<if>
			<isset var="workflowActivityComment" />
			<then>
				<var name="key_workflowActivityComment" value="${workflowActivityComment}" />

				<execute function="Type#clickAtType" locator1="MyWorkflowTasks#DIALOG_BOX_COMMENT_FIELD" value1="${workflowActivityComment}" />
			</then>
		</if>

		<execute macro="Workflow#confirmWorkflowAction" />

		<execute function="Pause" locator1="1000" />

		<execute macro="Workflow#viewCompletedTask">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute function="Pause" locator1="1000" />

		<if>
			<isset var="workflowActivityComment" />
			<then>
				<execute macro="Workflow#gotoCompletedFilter" />

				<execute macro="Workflow#_viewActivities">
					<var name="workflowActivityComment" value="${workflowActivityComment}" />
					<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
				</execute>
			</then>
		</if>
	</command>

	<command name="approveTaskByDetails">
		<execute macro="Workflow#viewTaskAssignedToMe">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute macro="Workflow#viewTaskAssignedToMeDetails">
			<var name="workflowAssetContent" value="${workflowAssetContent}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssignedTo" value="${workflowAssignedTo}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute function="Click#waitForMenuToggleJSClick" locator1="Icon#BODY_VERTICAL_ELLIPSIS" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Approve">
			<var name="key_menuItem" value="Approve" />
		</execute>

		<execute macro="Workflow#confirmWorkflowAction" />

		<execute macro="Workflow#viewCompletedTask">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>
	</command>

	<command name="assignByDetails">
		<execute function="Click#waitForMenuToggleJSClick" locator1="Icon#BODY_VERTICAL_ELLIPSIS" />

		<execute function="AssertClick" locator1="MyWorkflowTasksTask#MENU_ASSIGN" value1="Assign to..." />

		<execute function="AssertTextEquals" locator1="MyWorkflowTasks#DIALOG_BOX_TITLE" value1="Assign to..." />

		<execute macro="IFrame#selectConfigurationFrame" />

		<execute function="Select" locator1="MyWorkflowTasks#DIALOG_BOX_ASSIGN_TO_SELECT" value1="${user}" />

		<if>
			<isset var="comment" />
			<then>
				<execute function="Type" locator1="MyWorkflowTasks#DIALOG_BOX_COMMENT_FIELD" value1="${comment}" />
			</then>
		</if>

		<execute macro="Button#clickDone" />

		<execute macro="Workflow#waitForSuccessMessageHide" />
	</command>

	<command name="assignToMeTaskByActions">
		<execute macro="Workflow#viewTaskAssignedToMyRoles">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute macro="Workflow#gotoTaskAssignedToMyRolesActionsCmd">
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Assign to Me">
			<var name="key_menuItem" value="Assign to Me" />
		</execute>

		<execute macro="IFrame#selectConfigurationFrame" />

		<if>
			<isset var="workflowActivityComment" />
			<then>
				<execute function="Type#clickAtType" locator1="MyWorkflowTasks#DIALOG_BOX_COMMENT_FIELD" value1="${workflowActivityComment}" />
			</then>
		</if>

		<execute macro="Workflow#confirmWorkflowAction" />

		<execute macro="Workflow#viewTaskAssignedToMe">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<if>
			<isset var="workflowActivityComment" />
			<then>
				<execute macro="Workflow#_viewActivities">
					<var name="workflowActivityComment" value="${workflowActivityComment}" />
					<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
				</execute>
			</then>
		</if>
	</command>

	<command name="assignToMeTaskByDetails">
		<execute macro="Workflow#viewTaskAssignedToMyRoles">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute macro="Workflow#viewTaskAssignedToMyRolesDetails">
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute macro="Workflow#clickAssignToMeByDetails" />

		<execute macro="Workflow#confirmWorkflowAction" />

		<execute function="Click" locator1="Icon#ANGLE_LEFT_BACK" />

		<execute macro="Workflow#viewTaskAssignedToMe">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>
	</command>

	<command name="clickApproveByDetails">
		<execute function="Click#waitForMenuToggleJSClick" locator1="Icon#BODY_VERTICAL_ELLIPSIS" />

		<execute function="AssertClick" locator1="MyWorkflowTasksTask#MENU_APPROVE" value1="Approve" />
	</command>

	<command name="clickAssignToMeByDetails">
		<execute function="Click#waitForMenuToggleJSClick" locator1="Icon#BODY_VERTICAL_ELLIPSIS" />

		<execute function="AssertClick" locator1="MyWorkflowTasksTask#MENU_ASSIGN_TO_ME" value1="Assign to Me" />
	</command>

	<command name="configureWorkflow">
		<execute function="AssertClick" locator1="Button#EDIT_ROW" value1="Edit">
			<var name="key_rowEntry" value="${workflowResourceValue}" />
		</execute>

		<execute function="Select" locator1="WorkflowConfiguration#RESOURCE_TABLE_SELECT" value1="${workflowDefinition}">
			<var name="key_workflowResourceValue" value="${workflowResourceValue}" />
		</execute>

		<execute function="AssertClick" locator1="WorkflowConfiguration#RESOURCE_TABLE_SAVE" value1="Save">
			<var name="key_workflowResourceValue" value="${workflowResourceValue}" />
		</execute>

		<execute macro="Alert#viewSuccessMessage" />

		<execute macro="Workflow#viewConfigurationSelected">
			<var name="workflowDefinition" value="${workflowDefinition}" />
			<var name="workflowResourceValue" value="${workflowResourceValue}" />
		</execute>
	</command>

	<command name="configureWorkflowViaFolderInfo">
		<execute function="Click" locator1="Icon#INFO" />

		<execute function="Click#waitForMenuToggleJSClick" locator1="Icon#BODY_VERTICAL_ELLIPSIS" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Edit">
			<var name="key_menuItem" value="Edit" />
		</execute>

		<execute function="Select" locator1="AssetWorkflow#ALL_STRUCTURES_WORKFLOW_SELECT" value1="${workflowDefinition}" />

		<execute macro="PortletEntry#save" />
	</command>

	<command name="configureWorkflowViaPortlet">
		<execute function="Click" locator1="Icon#HEADER_VERTICAL_ELLIPSIS" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Edit">
			<var name="key_menuItem" value="Edit" />
		</execute>

		<execute function="Select" locator1="AssetWorkflow#ALL_STRUCTURES_WORKFLOW_SELECT" value1="${workflowDefinition}" />

		<execute macro="PortletEntry#save" />
	</command>

	<command name="confirmWorkflowAction">
		<execute macro="IFrame#selectConfigurationFrame" />

		<execute function="AssertElementPresent" locator1="Button#DONE" />

		<execute function="Click" locator1="Button#DONE" />

		<execute macro="Workflow#waitForSuccessMessageHide" />
	</command>

	<command name="deleteWorkflowDefinition">
		<var name="key_workflowDefinitionTitle" value="${workflowDefinitionTitle}" />

		<execute function="Click" locator1="WorkflowDefinition#DEFINITION_TABLE_SPECIFIC_DEFINITION_ELLIPSIS" />

		<execute function="AssertClickNoError" locator1="MenuItem#ANY_MENU_ITEM" value1="Delete">
			<var name="key_menuItem" value="Delete" />
		</execute>

		<execute function="AssertTextEquals" locator1="WorkflowDefinition#DIALOG_BOX_TITLE" value1="Delete workflow?" />

		<execute function="AssertTextEquals" locator1="WorkflowDefinition#DIALOG_BOX_BODY" value1="A deleted workflow cannot be recovered." />

		<execute macro="Button#clickDelete" />

		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Workflow deleted successfully." />

		<execute macro="Workflow#waitForSuccessMessageHide" />

		<execute function="AssertElementNotPresent" locator1="WorkflowDefinition#DEFINITION_TABLE_TITLE" />
	</command>

	<command name="gotoAssetViaTableTitle">
		<var name="key_workflowAssetTitle" value="${workflowAssetTitle}" />

		<execute function="AssertClick" locator1="MyWorkflowTasks#TABLE_GENERIC_ASSET_TITLE" value1="${workflowAssetTitle}" />
	</command>

	<command name="gotoCompletedFilter">
		<execute function="Pause" locator1="5000" />

		<execute function="Click" locator1="Dropdown#FILTER" />

		<var name="key_filterValue" value="Completed" />

		<execute function="AssertClick" locator1="Dropdown#FILTER_VALUES" value1="Completed" />
	</command>

	<command name="gotoCompletedTaskAssignedToMePreview">
		<var name="key_workflowAssetTitle" value="${workflowAssetTitle}" />

		<execute function="AssertClick" locator1="MyWorkflowTasks#ASSIGNED_TO_ME_TABLE_ASSET_TITLE" value1="${workflowAssetTitle}" />

		<execute macro="Workflow#gotoPreviewView" />
	</command>

	<command name="gotoEditWorkflowDefinition">
		<execute macro="LexiconEntry#gotoEntryMenuItem">
			<var name="menuItem" value="Edit" />
			<var name="rowEntry" value="${workflowDefinitionTitle}" />
		</execute>
	</command>

	<command name="gotoPreviewEdit">
		<execute function="Click" locator1="WorkflowSubmissionsTask#PREVIEW_EDIT" />

		<execute function="SelectFrame" locator1="IFrame#EDIT_ASSET" />
	</command>

	<command name="gotoPreviewView">
		<execute function="AssertVisible" locator1="WorkflowSubmissionsTask#PREVIEW_VIEW" />

		<execute function="Click" locator1="WorkflowSubmissionsTask#PREVIEW_VIEW" />

		<execute function="AssertElementNotPresent" locator1="WorkflowSubmissionsTask#PREVIEW_VIEW" />
	</command>

	<command name="gotoTaskAssignedToMeActionsCmd">
		<var name="key_workflowAssetTitle" value="${workflowAssetTitle}" />
		<var name="key_workflowTask" value="${workflowTask}" />

		<execute function="Click" locator1="MyWorkflowTasks#ASSIGNED_TO_ME_TABLE_ACTIONS" />
	</command>

	<command name="gotoTaskAssignedToMyRolesActionsCmd">
		<var name="key_workflowAssetTitle" value="${workflowAssetTitle}" />
		<var name="key_workflowTask" value="${workflowTask}" />

		<execute function="Click" locator1="MyWorkflowTasks#ASSIGNED_TO_MY_ROLES_TABLE_ACTIONS" />
	</command>

	<command name="gotoTaskDetails">
		<var name="key_workflowAssetTitle" value="${workflowAssetTitle}" />

		<execute function="AssertClick" locator1="MyWorkflowTasks#ASSIGNED_TO_ME_TABLE_ASSET_TITLE" value1="${workflowAssetTitle}" />
	</command>

	<command name="gotoViewCompletedAssetViaMySubmissions">
		<execute macro="Workflow#gotoCompletedFilter" />

		<var name="key_workflowAssetTitle" value="${workflowAssetTitle}" />

		<execute function="Click" locator1="WorkflowSubmissions#COMPLETED_TABLE_ASSET_TITLE" />

		<execute macro="Workflow#gotoPreviewView" />
	</command>

	<command name="publishExistingWorkflowDefinition">
		<var name="key_workflowDefinitionTitle" value="${workflowDefinitionTitle}" />

		<execute function="Click" locator1="WorkflowDefinition#DEFINITION_TABLE_SPECIFIC_DEFINITION_ELLIPSIS" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Edit">
			<var name="key_menuItem" value="Edit" />
		</execute>

		<execute macro="Workflow#viewDefinitionPublicationStatus">
			<var name="status" value="Not Published" />
		</execute>

		<execute macro="PortletEntry#publish" />

		<execute macro="Workflow#waitForSuccessMessageHide" />

		<execute macro="Workflow#viewDefinitionPublicationStatus">
			<var name="status" value="Published" />
		</execute>
	</command>

	<command name="publishInvalidWorkflowDefinition">
		<execute function="AssertClickNoError" locator1="Button#PUBLISH" value1="Publish" />

		<execute macro="Alert#viewErrorMessage">
			<var name="errorMessage" value="${errorMessage}" />
		</execute>
	</command>

	<command name="reactivateInactiveWorkflowDefinition">
		<var name="key_workflowDefinitionTitle" value="${workflowDefinitionTitle}" />

		<execute function="Click" locator1="WorkflowDefinition#DEFINITION_TABLE_SPECIFIC_DEFINITION_ELLIPSIS" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Publish">
			<var name="key_menuItem" value="Publish" />
		</execute>

		<execute macro="Workflow#waitForSuccessMessageHide" />
	</command>

	<command name="rejectTaskByActions">
		<execute macro="Workflow#viewTaskAssignedToMe">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute macro="Workflow#gotoTaskAssignedToMeActionsCmd">
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute function="AssertClick#pauseAssertTextClickAt" locator1="MenuItem#ANY_MENU_ITEM" value1="Reject">
			<var name="key_menuItem" value="Reject" />
		</execute>

		<if>
			<isset var="workflowActivityComment" />
			<then>
				<var name="key_workflowActivityComment" value="${workflowActivityComment}" />

				<execute function="Type#clickAtType" locator1="MyWorkflowTasks#DIALOG_BOX_COMMENT_FIELD" value1="${workflowActivityComment}" />
			</then>
		</if>

		<execute macro="Workflow#confirmWorkflowAction" />

		<execute function="SelectFrame" value1="relative=top" />

		<execute macro="Workflow#viewCompletedTask">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>
	</command>

	<command name="rejectTaskByDetails">
		<execute macro="Workflow#viewTaskAssignedToMe">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute macro="Workflow#viewTaskAssignedToMeDetails">
			<var name="workflowAssetContent" value="${workflowAssetContent}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssignedTo" value="${workflowAssignedTo}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute function="Click#waitForMenuToggleJSClick" locator1="Icon#BODY_VERTICAL_ELLIPSIS" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Reject">
			<var name="key_menuItem" value="Reject" />
		</execute>

		<execute macro="Workflow#confirmWorkflowAction" />

		<execute function="SelectFrame" value1="relative=top" />

		<execute macro="Workflow#viewCompletedTask">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>
	</command>

	<command name="resubmitTaskByActions">
		<execute macro="Workflow#viewTaskAssignedToMe">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute macro="Workflow#gotoTaskAssignedToMeActionsCmd">
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute function="AssertClick#pauseAssertTextClickAt" locator1="MenuItem#ANY_MENU_ITEM" value1="Resubmit">
			<var name="key_menuItem" value="Resubmit" />
		</execute>

		<execute macro="Workflow#confirmWorkflowAction" />

		<execute macro="Workflow#viewCompletedTask">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>
	</command>

	<command name="resubmitTaskByDetails">
		<execute macro="Workflow#viewTaskAssignedToMe">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute macro="Workflow#viewTaskAssignedToMeDetails">
			<var name="workflowAssetContent" value="${workflowAssetContent}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssignedTo" value="${workflowAssignedTo}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>

		<execute function="Click#waitForMenuToggleJSClick" locator1="Icon#BODY_VERTICAL_ELLIPSIS" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Resubmit">
			<var name="key_menuItem" value="Resubmit" />
		</execute>

		<execute macro="Workflow#confirmWorkflowAction" />

		<execute macro="Workflow#viewCompletedTask">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
			<var name="workflowAssetTitle" value="${workflowAssetTitle}" />
			<var name="workflowAssetType" value="${workflowAssetType}" />
			<var name="workflowTask" value="${workflowTask}" />
		</execute>
	</command>

	<command name="saveDefinition">
		<execute function="AssertClick" locator1="KaleoDesigner#SAVE_BUTTON" value1="Save" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="tearDownPortalWorkflow">
		<execute macro="ProductMenu#gotoPortlet">
			<var name="category" value="Configuration" />
			<var name="panel" value="Control Panel" />
			<var name="portlet" value="Workflow" />
		</execute>

		<execute macro="Navigator#gotoNavItem">
			<var name="navItem" value="Schemes" />
		</execute>

		<execute macro="Workflow#_tearDownWorkflow" />

		<execute macro="LexiconEntry#gotoEntryMenuItem">
			<var name="menuItem" value="Edit" />
			<var name="rowEntry" value="User" />
		</execute>

		<execute function="Select" locator1="WorkflowConfiguration#RESOURCE_TABLE_SELECT" value1="No Workflow">
			<var name="key_workflowResourceValue" value="User" />
