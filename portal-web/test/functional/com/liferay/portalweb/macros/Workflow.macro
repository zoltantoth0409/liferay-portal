		SelectFrame(locator1 = "IFrame#SELECT_ASSIGN_TO");

		Select(
			locator1 = "MyWorkflowTasks#DIALOG_BOX_ASSIGN_TO_SELECT",
			value1 = "${user}"
		);

		if (isSet(comment)) {
			Type(
				locator1 = "MyWorkflowTasks#DIALOG_BOX_COMMENT_FIELD",
				value1 = "${comment}"
			);
		}

		Button.clickDone();

		Alert.waitForSuccessMessageHide();
