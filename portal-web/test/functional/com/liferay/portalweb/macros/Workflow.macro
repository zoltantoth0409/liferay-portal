	macro publishInvalidWorkflowDefinition {
		if (isSet(update)) {
			AssertClickNoError(
				locator1 = "Button#UPDATE",
				value1 = "Update");
		}
		else {
			AssertClickNoError(
				locator1 = "Button#PUBLISH",
				value1 = "Publish");
		}

		Alert.viewErrorMessage(errorMessage = "${errorMessage}");
	}

