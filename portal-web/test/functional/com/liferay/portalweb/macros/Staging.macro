	macro publishCustomPublication {
		Pause(locator1 = "3000");

		LexiconEntry.gotoAdd();

		if ("${rangeAll}" == "true") {
			Click(locator1 = "Radio#CHOOSE_CONTENT_ALL_CHANGE");

			Click(locator1 = "Link#REFRESH_COUNTS");
		}

		if ("${remoteStaging}" == "true") {
