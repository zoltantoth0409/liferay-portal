	macro deleteCP {
		Type.sendKeys(
			locator1 = "NavBar#BASIC_SEARCH_FIELD",
			value1 = "Name");

		Click(locator1 = "Button#SUBMIT");

		var key_roleName = "${roleName}";

		Click(locator1 = "Roles#ROLE_TABLE_ACTIONS");

		MenuItem.clickNoError(menuItem = "Delete");

		Confirm(value1 = "Are you sure you want to delete this role? It will be deleted immediately. If it is a reviewer role, its task assignments will be deleted along with it.");
