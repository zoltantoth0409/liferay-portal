			if ("${define}" == "true") {
				ScrollBy(value1 = "0, 400");

				Check(locator1 = "RolesPermissions#${permissionDefinitionKey}");
			}
			else if ("${remove}" == "true") {
				Uncheck(locator1 = "RolesPermissions#${permissionDefinitionKey}");
