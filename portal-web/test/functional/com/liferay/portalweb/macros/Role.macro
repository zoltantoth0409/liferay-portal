definition {

	macro addCP {
		if ("${roleType}" == "Organization") {
			Navigator.gotoNavItem(navItem = "Organization Roles");
		}
		else if ("${roleType}" == "Site") {
			Navigator.gotoNavItem(navItem = "Site Roles");
		}
		else {
			Navigator.gotoNavItem(navItem = "Regular Roles");
		}

		if (("${roleType}" == "Organization") || ("${roleType}" == "Site")) {
			var key_roleType = "${roleType}";
		}
		else {
			var key_roleType = "Regular";
		}

		Breadcrumb.viewEntry(breadcrumbName = "${key_roleType} Roles");

		LexiconEntry.gotoAdd();

		AssertTextEquals(
			locator1 = "Portlet#HEADER",
			value1 = "New Role");

		PortletEntry.inputName(name = "${roleName}");

		if (isSet(roleDescription)) {
			PortletEntry.inputDescription(description = "${roleDescription}");
		}

		if (isSet(roleTitle)) {
			PortletEntry.inputTitle(title = "${roleTitle}");
		}

		AssertClick(
			locator1 = "Button#SUBMIT",
			value1 = "Save");

		Alert.viewSuccessMessage();
	}

	macro addPermissions {
		for (var permissionDefinitionKey : list "${permissionDefinitionKeys}") {
			Check(locator1 = "RolesPermissions#${permissionDefinitionKey}");
		}

		ScrollWebElementIntoView(locator1 = "RolesPermissions#SAVE_BUTTON");

		PortletEntry.save();
	}

	macro addStagingAdminRole {
		var roleName = "Staging Admin";

		Role.addCP(
			roleDescription = "${roleDescription}",
			roleName = "${roleName}",
			roleTitle = "${roleTitle}",
			roleType = "Site");

		ProductMenu.gotoPortlet(
			category = "Users",
			panel = "Control Panel",
			portlet = "Roles");

		Role.definePermissionsCP(
			permissionDefinitionKeys = "SITE_ADMIN_PAGES_SITE_PAGES_GENERAL_PERMISSIONS_ACTION_CHECKBOX,SITE_ADMIN_PAGES_SITE_PAGES_RESOURCE_PERMISSIONS_SITE_ACTION_CHECKBOX,SITE_ADMIN_PAGES_SITE_PAGES_RESOURCE_PERMISSIONS_PAGE_ACTION_CHECKBOX,SITE_ADMIN_PAGES_SITE_PAGES_RESOURCE_PERMISSIONS_PAGE_VARIATION_ACTION_CHECKBOX,SITE_ADMIN_PAGES_SITE_PAGES_RESOURCE_PERMISSIONS_SITE_PAGES_VARIATION_ACTION_CHECKBOX",
			roleName = "${roleName}",
			roleType = "Site");
	}

	macro assignMembersUserGroupsCP {
		var key_roleName = "${roleName}";

		Type.sendKeys(
			locator1 = "NavBar#BASIC_SEARCH_FIELD",
			value1 = "${roleName}");

		Click(locator1 = "Button#SUBMIT");

		AssertClick(
			locator1 = "Roles#ROLE_TABLE_TITLE_LINK",
			value1 = "${roleName}");

		LexiconEntry.gotoAdd();

		SelectFrame(locator1 = "IFrame#SELECT_ASSIGNEES");

		AssertClick(
			locator1 = "NavBar#USER_GROUPS",
			value1 = "User Groups");

		var key_userGroupName = "${userGroupName}";

		Type(
			locator1 = "NavBar#BASIC_SEARCH_FIELD",
			value1 = "${userGroupName}");

		Click(locator1 = "Button#SUBMIT");

		AssertTextEquals(
			locator1 = "RolesAssignMembers#USER_GROUP_TABLE_NAME",
			value1 = "${userGroupName}");

		Check(locator1 = "RolesAssignMembers#USER_GROUP_TABLE_CHECKBOX");

		SelectFrameTop(value1 = "relative=top");

		AssertClick(
			locator1 = "Button#ADD",
			value1 = "Add");

		Alert.viewSuccessMessage();
	}

	macro assignRegRoleCP {
