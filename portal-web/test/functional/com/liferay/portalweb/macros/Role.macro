					value1 = "${siteNameScope}");

				var key_siteNameScope = "${siteNameScope}";
				var permissionDefinitionKeySiteScope = StringUtil.replace("${permissionDefinitionKey}", "CHECKBOX", "SITE_SCOPE");

				SelectFrame(value1 = "relative=top");

				AssertTextEquals.assertPartialText(
					locator1 = "RolesPermissions#${permissionDefinitionKeySiteScope}",
					value1 = "${siteNameScope}");
			}
		}

		if ("${confirmation}" == "true") {
			ScrollWebElementIntoView(locator1 = "RolesPermissions#SAVE_BUTTON");

			AssertClickNoError(
				locator1 = "RolesPermissions#SAVE_BUTTON",
				value1 = "Save");
		}
		else {
			ScrollWebElementIntoView(locator1 = "RolesPermissions#SAVE_BUTTON");

			AssertClick(
				locator1 = "RolesPermissions#SAVE_BUTTON",
				value1 = "Save");
		}

		if ("${removeLink}" == "true") {
			Confirm.waitForConfirmation(value1 = "Updating the chosen permissions will overwrite the respective individually defined permissions for one or more of these resource types: Bookmarks Entry, Bookmarks Folder, Calendar Booking, Calendar, Calendar Resource");
		}

		Alert.viewSuccessMessage();
