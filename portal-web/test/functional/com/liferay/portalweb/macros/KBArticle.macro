		<execute function="AssertTextEquals" locator1="KnowledgeBase#DISPLAY_CONTENT" value1="${kbChildArticleContent}" />
	</command>

	<command name="viewCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertTextEquals" locator1="KnowledgeBase#DESCRIPTIVE_VIEW_ARTICLE_TITLE" value1="${kbArticleTitle}" />

		<if>
			<equals arg1="${kbWorkflowStatus}" arg2="Pending" />
			<then>
				<execute function="AssertTextEquals" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_STATUS" value1="Pending" />
			</then>
		</if>

		<if>
			<equals arg1="${kbStatus}" arg2="Draft" />
			<then>
				<execute function="AssertTextEquals" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_STATUS" value1="Draft" />
			</then>
		</if>

		<execute function="AssertClick" locator1="KnowledgeBase#DESCRIPTIVE_VIEW_ARTICLE_TITLE" value1="${kbArticleTitle}" />

		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#CONTENT" value1="${kbArticleContent}" />

		<if>
			<isset var="kbChildArticleTitle" />
			<then>
				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#ADMIN_VIEW_CHILD_ARTICLE_TITLE" value1="${kbChildArticleTitle}" />
				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#ADMIN_VIEW_CHILD_ARTICLE_CONTENT" value1="${kbChildArticleContent}" />
			</then>
		</if>

		<if>
			<isset var="kbArticleRelatedAssetTitle" />
			<then>
				<execute function="AssertTextEquals" locator1="AssetRelatedAssets#HEADER" value1="Related Assets:" />
				<execute function="AssertTextEquals" locator1="AssetRelatedAssets#KB_RELATED_ASSETS_LINKS" value1="${kbArticleRelatedAssetTitle}">
					<var name="key_kbArticleRelatedAssetTitle" value="${kbArticleRelatedAssetTitle}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="categoryName" />
			<then>
				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#CATEGORY_HEADER" value1="${vocabularyName}: ${categoryName}" />
				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#CATEGORY_LINKS" value1="${categoryName}">
					<var name="key_categoryName" value="${categoryName}" />
				</execute>
			</then>
		</if>

		<if>
			<equals arg1="noAttachment" arg2="true" />
			<then>
				<execute function="AssertAlertNotPresent" locator1="KnowledgeBaseArticle#ATTACHMENTS_SAVED_ATTACHMENTS_CONTAINER" />
			</then>
		</if>
	</command>

	<command name="viewCPWithHTML">
		<execute function="AssertClick" locator1="KnowledgeBase#DESCRIPTIVE_VIEW_ARTICLE_TITLE" value1="${kbArticleTitle}">
			<var name="key_kbArticleTitle" value="${kbArticleTitle}" />
		</execute>

		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#HTML_BOLD_CONTENT" value1="${kbArticleContent}" />
	</command>

	<command name="viewDefaultCP">
		<execute function="AssertTextEquals#assertPartialText" locator1="Message#EMPTY_INFO" value1="No articles were found." />
		<execute function="AssertElementNotPresent" locator1="KnowledgeBase#ARTICLE_TABLE_KB_GENERIC" />
	</command>

	<command name="viewDefaultKBDisplaySettingsPG">
		<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#SUBSCRIBE" />
		<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#HISTORY" />
		<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#PRINT" />
	</command>

	<command name="viewDefaultViaKBDisplayPG">
		<execute function="AssertTextEquals" locator1="Message#INFO" value1="Please configure this portlet to make it visible to all users." />
		<execute function="AssertElementNotPresent" locator1="KnowledgeBase#DISPLAY_TITLE" />
	</command>

	<command name="viewFriendlyURLCP">
		<execute function="Click" locator1="Icon#ROW_VERTICAL_ELLIPSIS_2">
			<var name="key_rowEntry" value="${kbArticleTitle}" />
		</execute>

		<execute macro="MenuItem#click">
			<var name="menuItem" value="Edit" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Configuration" />
		</execute>

		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#EDIT_FRIENDLY_URL_FIELD" value1="${kbArticleFriendlyURL}" />
	</command>

	<command name="viewImageViaContentCmd">
		<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#CONTENT_IMAGE" />
	</command>

	<command name="viewKBArticlePriorityCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertTextEquals" locator1="KnowledgeBase#DESCRIPTIVE_VIEW_ARTICLE_TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBase#DESCRIPTIVE_VIEW_ARTICLE_PRIORITY" value1="Priority ${kbPriority}" />
	</command>

	<command name="viewKBArticleWithAttachmentCP">
		<execute function="Click" locator1="Icon#ROW_VERTICAL_ELLIPSIS">
			<var name="key_rowEntry" value="${kbArticleTitle}" />
		</execute>

		<execute macro="MenuItem#click">
			<var name="menuItem" value="Edit" />
		</execute>

		<execute function="AssertTextEquals" locator1="Message#INFO" value1="A new version is created automatically if this content is modified." />
		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="AlloyEditor#EDITOR" value1="${kbArticleContent}">
			<var name="key_editor" value="content" />
		</execute>

		<if>
			<condition function="IsElementNotPresent" locator1="KnowledgeBaseArticle#ATTACHMENTS_HEADER_EXPANDED" />
			<then>
				<execute function="Click" locator1="KnowledgeBaseArticle#ATTACHMENTS_HEADER_COLLAPSED" />
			</then>
		</if>

		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#ATTACHMENTS_SAVED_ATTACHMENTS_CONTAINER" value1="${kbArticleAttachment}" />
	</command>

	<command name="viewKBDisplayPG">
		<if>
			<isset var="kbArticleFriendlyURL" />
			<then>
				<execute function="AssertLocation" value1="http://localhost:8080/web/guest/knowledge-base-display/-/knowledge_base/${kbArticleFriendlyURL}" />
			</then>
		</if>

		<if>
			<equals arg1="${sourceURL}" arg2="true" />
			<then>
				<execute function="AssertClick" locator1="KnowledgeBaseArticle#EDIT_ON_GITHUB" value1="Edit On GitHub" />

				<execute function="SelectWindow" locator1="Google#PAGE_TITLE">
					<var name="key_pageTitle" value="Google" />
				</execute>

				<var method="selenium#getLocation()" name="url" />

				<if>
					<not>
						<equals arg1="${url}" arg2="https://www.google.com/[#]gws_rd=ssl" />
					</not>
					<then>
						<execute function="AssertLocation" value1="https://www.google.com/?gws_rd=ssl" />
					</then>
					<else>
						<execute function="AssertLocation" value1="https://www.google.com/[#]gws_rd=ssl" />
					</else>
				</if>

				<execute function="Close" locator1="Google#PAGE_TITLE">
					<var name="key_pageTitle" value="Google" />
				</execute>
			</then>
		</if>

		<if>
			<equals arg1="${subscribe}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#SUBSCRIBE" />
			</then>
			<elseif>
				<equals arg1="${subscribe}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#SUBSCRIBE" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${history}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#HISTORY" />
			</then>
			<elseif>
				<equals arg1="${history}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#HISTORY" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${print}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#PRINT" />
			</then>
			<elseif>
				<equals arg1="${print}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#PRINT" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="viewNavigationLinksPG">
		<if>
			<equals arg1="${previous}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#PREVIOUS" />
			</then>
			<elseif>
				<equals arg1="${previous}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#PREVIOUS" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${next}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#NEXT" />
			</then>
			<elseif>
				<equals arg1="${next}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#NEXT" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="viewNoHierarchyPG">
		<execute function="AssertElementNotPresent" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_PARENT">
			<var name="key_kbArticleTitle" value="${kbArticleTitle}" />
		</execute>

		<execute function="AssertElementPresent" locator1="KnowledgeBase#FOLDER_NAVIGATION_MENULIST" />
	</command>

	<command name="viewNoPGViaKBDisplay">
		<execute function="AssertTextNotPresent" value1="${kbArticleTitle}" />
	</command>

	<command name="viewParentArticleHierarchyViaKBDisplayPG">
		<var name="count" value="0" />

		<while>
			<condition function="IsElementPresent" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_GENERIC" />
			<then>
				<var method="MathUtil#sum('${count}', '1')" name="count" />
				<var name="kbParentTitle" value="${kbArticleTitle} ${count}" />

				<if>
					<condition function="IsElementPresent" locator1="xpath=//div[@class='kbarticle-navigation']/ul[${count}]" />
					<then>
						<execute function="AssertClick" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_PARENT" value1="${kbParentTitle}">
							<var name="key_kbArticleTitle" value="${kbParentTitle}" />
						</execute>
					</then>
				</if>
			</then>
		</while>
	</command>

	<command name="viewPG">
		<execute function="AssertTextEquals#assertPartialText" locator1="KnowledgeBase#DISPLAY_TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBase#DISPLAY_CONTENT" value1="${kbArticleContent}" />

		<if>
			<isset var="customFieldName" />
			<then>
				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#CUSTOM_FIELD_NAME" value1="${customFieldName}" />
				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#CUSTOM_FIELD_VALUE" value1="${customFieldValue}" />
			</then>
		</if>
	</command>

	<command name="viewPGViaAP">
		<var name="key_assetContent" value="${kbArticleContent}" />
		<var name="key_assetTitle" value="${kbArticleTitle}" />

		<execute function="AssertTextEquals" locator1="AP#ASSET_ABSTRACT_TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals#assertPartialText" locator1="AP#ASSET_ABSTRACT_DESCRIPTION" value1="${kbArticleContent}" />
	</command>

	<command name="viewSourceURLCP">
		<if>
			<isset var="kbChildArticleTitle" />
			<then>
				<execute function="Click" locator1="Icon#HEADER_VERTICAL_ELLIPSIS" />
			</then>
			<else>
				<execute function="Click" locator1="Icon#ROW_VERTICAL_ELLIPSIS">
					<var name="key_rowEntry" value="${kbArticleTitle}" />
				</execute>
			</else>
		</if>

		<execute macro="MenuItem#click">
			<var name="menuItem" value="Edit" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Configuration" />
		</execute>

		<execute function="AssertTextEquals" locator1="CKEditor#SOURCE_URL" value1="${kbArticleSourceURL}" />
	</command>

	<command name="viewSourceURLPG">
		<execute function="AssertClick" locator1="KnowledgeBase#KB_TOOLS" value1="Edit">
			<var name="key_kbTool" value="Edit" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Configuration" />
		</execute>

		<execute function="AssertTextEquals" locator1="CKEditor#SOURCE_URL" value1="${kbArticleSourceURL}" />
	</command>

	<command name="viewUploadFailed">
		<if>
			<equals arg1="${kbImportFailed}" arg2="true" />
			<then>
				<execute macro="Alert#viewRequestFailedToComplete" />
			</then>
			<else>
				<execute function="AssertTextNotPresent" value1="Your request failed to complete." />
			</else>
		</if>
	</command>

	<command name="viewViaKBDisplayPG">
		<execute function="AssertTextEquals#assertPartialText" locator1="KnowledgeBase#DISPLAY_TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBase#DISPLAY_CONTENT" value1="${kbArticleContent}" />

		<if>
			<isset var="kbArticleRelatedAssetTitle" />
			<then>
				<execute function="AssertTextEquals" locator1="AssetRelatedAssets#HEADER" value1="Related Assets:" />
				<execute function="AssertTextEquals" locator1="AssetRelatedAssets#KB_RELATED_ASSETS_LINKS" value1="${kbArticleRelatedAssetTitle}">
					<var name="key_kbArticleRelatedAssetTitle" value="${kbArticleRelatedAssetTitle}" />
				</execute>
			</then>
		</if>
	</command>

	<command name="viewViaNextLinkPG">
		<execute function="AssertClick" locator1="KnowledgeBaseArticle#NEXT" value1="${kbArticleTitle}" />

		<execute function="AssertTextPresent" locator1="KnowledgeBase#DISPLAY_CONTENT" value1="${kbArticleContent}" />
	</command>
</definition>