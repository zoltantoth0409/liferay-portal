	macro gotoActionsCmd {
		Pause(locator1 = "1000");

		Type(locator1 = "NavBar#BASIC_SEARCH_FIELD", value1 = "${orgName}");

		Click(locator1 = "Button#SUBMIT");

		AssertVisible(
			locator1 = "UsersAndOrganizations#ORGANIZATION_TABLE_NAME_GENERIC"
		);

		var key_orgName = "${orgName}";

		if (IsElementNotPresent(locator1 = "UsersAndOrganizations#ORGANIZATION_TABLE_NAME")) {
			Pagination.changePagination(itemsPerPage = "30");
		}

		AssertTextEquals(
			locator1 = "UsersAndOrganizations#ORGANIZATION_TABLE_NAME",
			value1 = "${orgName}"
		);

		Click(locator1 = "UsersAndOrganizations#ORGANIZATION_TABLE_ACTIONS");
	}

	macro gotoCP {
		Type(locator1 = "TextInput#SEARCH", value1 = "${searchToken}");

		Click(locator1 = "Button#SUBMIT");

		var key_orgName = "${orgName}";

		AssertTextEquals(
			locator1 = "UsersAndOrganizations#ORGANIZATION_TABLE_NAME",
			value1 = "${orgName}"
		);

		AssertClick(
			locator1 = "UsersAndOrganizations#ORGANIZATION_TABLE_NAME",
			value1 = "${orgName}"
		);

		Breadcrumb.viewActiveEntry(breadcrumbName = "${orgName}");
	}

