	macro selectParentOrganizationCP {
		var key_orgName = "${orgName}";

		Click(locator1 = "UsersAndOrganizations#ORGANIZATION_TABLE_ACTIONS");

		AssertClick(
			locator1 = "MenuItem#ANY_MENU_ITEM",
			value1 = "Edit",
			key_menuItem = "Edit"
