	macro addWithLocalizedCountryCP {
		Navigator.gotoNavItem(navItem = "${navItem}");

		LexiconEntry.gotoAdd();

		if (IsElementPresent(locator1 = "MenuItem#ANY_MENU_ITEM", key_menuItem = "${localizedOption}")) {
