		<execute macro="FormFields#viewTextMultilineValue">
			<var name="fieldName" value="TextBox" />
			<var name="fieldValue" value="${localizedTextBox}" />
		</execute>
	</command>

	<command name="viewUserEditCP">
		<execute function="AssertTextEquals#assertPartialText" locator1="WC#ENTRY_DESCRIPTIVE_LAST_MODIFIED" value1="${userFirstLast}">
			<var name="key_webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute macro="WebContentNavigator#gotoEditCP">
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute function="AssertTextEquals#assertPartialText" locator1="WCEditWebContent#VERSION" value1="1.1" />
	</command>

	<command name="viewVideoViaWCD">
		<execute function="AssertElementPresent" locator1="WCD#WEB_CONTENT_VIDEO" />
	</command>

	<command name="viewWCViewableByPermissions">
		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="Web Content Title" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Permissions" />
		</execute>

		<execute function="AssertSelectedLabel" locator1="WCEditWebContent#PERMISSIONS_VIEWABLE_BY_SELECT" value1="Site Members" />
	</command>

	<command name="viewWithStructureCP">
		<execute macro="Portlet#changeDisplayStyle">
			<var name="displayStyle" value="descriptive" />
		</execute>

		<execute macro="WebContentNavigator#gotoEditCP">
			<var name="webContentTitle" value="${webContentTitle}" />
			<var name="webContentTitleEscapeCharacter" value="${webContentTitleEscapeCharacter}" />
		</execute>

		<execute function="AssertTextEquals" locator1="TextInput#TITLE" value1="${webContentTitle}" />

		<if>
			<isset var="description" />
			<then>
				<execute function="AssertTextEquals" locator1="AlloyEditor#EDITOR" value1="${description}">
					<var name="key_editor" value="description" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="webContentImage" />
			<then>
				<execute function="Click" locator1="Button#PREVIEW" />

				<execute function="SelectFrameTop" value1="relative=top" />

				<execute function="SikuliAssertElementPresent" locator1="DDMField#IMAGE_PREVIEW_PNG" />
			</then>
		</if>

		<if>
			<isset var="webContentText" />
			<then>
				<execute function="AssertTextEquals" locator1="DDMField#TEXT" value1="${webContentText}">
					<var name="key_fieldName" value="Text" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="webContentTextBox" />
			<then>
				<if>
					<isset var="fieldName" />
					<then>
						<execute function="AssertTextEquals" locator1="DDMField#TEXT_BOX" value1="${webContentTextBox}">
							<var name="key_fieldName" value="${fieldName}" />
						</execute>
					</then>
					<else>
						<execute function="AssertTextEquals" locator1="DDMField#TEXT_BOX" value1="${webContentTextBox}">
							<var name="key_fieldName" value="TextBox" />
						</execute>
					</else>
				</if>
			</then>
		</if>

		<if>
			<isset var="webContentTextBox2" />
			<then>
				<var name="key_fieldName" value="${fieldName2}" />

				<execute function="AssertTextEquals" locator1="DDMField#TEXT_BOX" value1="${webContentTextBox2}" />
			</then>
		</if>

		<if>
			<isset var="webContentTextBoxList" />
			<then>
				<var name="count" value="1" />

				<for list="${webContentTextBoxList}" param="webContentTextBox">
					<execute function="AssertTextEquals" locator1="DDMField#TEXT_BOX" value1="${webContentTextBox}">
						<var name="key_fieldName" value="heading${count}" />
					</execute>

					<var method="MathUtil#sum('${count}', '1')" name="count" />
				</for>
			</then>
		</if>
	</command>

	<command name="viewWithWorkflowCP">
		<execute macro="WebContentNavigator#gotoViewStatus">
			<var name="webContentStatus" value="Pending" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute function="AssertTextEquals" locator1="AssetWorkflow#WORKFLOW_STATUS" value1="Pending (${workflowTask})" />
		<execute function="AssertTextEquals" locator1="TextInput#TITLE" value1="${webContentTitle}" />
		<execute function="AssertTextEquals" locator1="AlloyEditor#EDITOR" value1="${webContentContent}">
			<var name="key_editor" value="content" />
		</execute>

		<execute function="SelectFrameTop" />
	</command>

	<command name="viewWithWorkflowPGViaAP">
		<execute function="AssertTextNotPresent" value1="${webContentTitle}" />
	</command>

	<command name="viewWithWorkflowPGViaWCD">
		<execute function="AssertTextEquals" locator1="Message#ERROR_3" value1="${webContentTitle} is not approved." />

		<execute function="AssertClick" locator1="Message#ERROR_3_LINK" value1="${webContentTitle} is not approved." />

		<execute function="AssertTextEquals" locator1="AssetWorkflow#WORKFLOW_STATUS" value1="Pending (${workflowTask})" />
		<execute function="AssertTextEquals" locator1="TextInput#TITLE" value1="${webContentTitle}" />
		<execute function="AssertTextEquals" locator1="AlloyEditor#EDITOR" value1="${webContentContent}">
			<var name="key_editor" value="content" />
		</execute>
	</command>
</definition>