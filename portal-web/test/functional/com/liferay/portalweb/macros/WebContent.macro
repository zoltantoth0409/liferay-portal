<definition>
	<command name="_gotoAddPGViaWCD">
		<execute function="MouseOver#mouseOverNotVisible" locator1="WCD#ADD_WEB_CONTENT" />

		<execute function="Click" locator1="WCD#ADD_WEB_CONTENT" />

		<var name="key_structureName" value="Basic Web Content" />

		<execute function="AssertClick" locator1="WCD#ADD_STRUCTURED_WEB_CONTENT" value1="Basic Web Content" />

		<execute function="SelectFrame" locator1="IFrame#EDIT_ASSET" />
	</command>

	<command name="addArticleCP">
		<execute macro="WebContentNavigator#gotoAddCP" />

		<execute macro="WebContent#addCP">
			<var name="webContentContent" value="${webContentContent}" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addCP">
		<if>
			<isset var="webContentTitle" />
			<then>
				<execute function="Click" locator1="TextInput#TITLE" />

				<execute macro="PortletEntry#inputTitle">
					<var name="title" value="${webContentTitle}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="webContentContent" />
			<then>
				<execute macro="AlloyEditor#addContent">
					<var name="content" value="${webContentContent}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="viewableBy" />
			<then>
				<execute macro="Panel#expandPanel">
					<var name="panel" value="Permissions" />
				</execute>

				<execute function="Select" locator1="WCEditWebContent#PERMISSIONS_VIEWABLE_BY_SELECT" value1="${viewableBy}" />
			</then>
		</if>
	</command>

	<command name="addDraft">
		<execute macro="WebContent#addCP">
			<var name="webContentContent" value="${webContentContent}" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute macro="Button#clickSaveAsDraft" />
	</command>

	<command name="addDraftPGViaAP">
		<execute macro="AssetPublisherPortlet#addAsset">
			<var name="assetType" value="Basic Web Content" />
		</execute>

		<execute macro="WebContent#addCP">
			<var name="webContentContent" value="${webContentContent}" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute macro="WebContent#saveAsDraftViaAP" />
	</command>

	<command name="addDraftPGViaWCD">
		<execute macro="WebContent#_gotoAddPGViaWCD" />

		<execute macro="WebContent#addDraft">
			<var name="webContentContent" value="${webContentContent}" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>
	</command>

	<command name="addExistingRelatedAssetCP">
		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Related Assets" />
		</execute>

		<execute function="AssertClick" locator1="AssetRelatedAssets#RELATED_ASSETS_SELECT" value1="Select" />

		<execute function="AssertClick" locator1="AssetRelatedAssets#RELATED_ASSETS_SELECT_MENULIST_ASSET" value1="${assetType}">
			<var name="key_assetType" value="${assetType}" />
		</execute>

		<execute function="SelectFrame" locator1="AssetRelatedAssets#SELECT_ASSET_IFRAME" />

		<execute macro="Search#searchCP">
			<var name="searchTerm" value="${assetTitle}" />
		</execute>

		<var name="key_assetTitle" value="${assetTitle}" />

		<execute function="AssertClick" locator1="AssetRelatedAssets#ASSET_TABLE_CHOOSE" value1="${assetTitle}" />

		<execute function="SelectFrameTop" />

		<execute function="AssertTextEquals" locator1="AssetRelatedAssets#RELATED_ASSETS_TABLE_TYPE" value1="${assetType}" />
		<execute function="AssertTextEquals" locator1="AssetRelatedAssets#RELATED_ASSETS_TABLE_TITLE" value1="${assetTitle}" />
		<execute function="AssertTextEquals#assertPartialText" locator1="AssetRelatedAssets#RELATED_ASSETS_TABLE_SCOPE" value1="${siteName}" />
		<execute function="AssertElementPresent" locator1="AssetRelatedAssets#RELATED_ASSETS_TABLE_REMOVE_LINK" />

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addExpirationDateCP">
		<execute macro="Panel#expandPanel">
			<var name="panel" value="Schedule" />
		</execute>

		<execute function="AssertChecked" locator1="Checkbox#NEVER_EXPIRE" value1="Never Expire" />

		<execute function="Uncheck" locator1="Checkbox#NEVER_EXPIRE" value1="Never Expire" />

		<execute function="Type#clickAtType" locator1="TextInput#EXPIRATION_DATE" value1="${expirationDate}" />
	</command>

	<command name="addFailWithoutRequiredCategoryAssetTypeCP">
		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="Web Content Title" />
		</execute>

		<execute macro="AlloyEditor#addContent">
			<var name="content" value="Web Content Content" />
		</execute>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="AssertElementPresent" locator1="Icon#ASTERISK" />

		<execute macro="Alert#viewRequestFailedToComplete" />

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Metadata" />
		</execute>

		<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR_2" value1="Please select at least one category for ${vocabularyName}." />
	</command>

	<command name="addFeed">
		<execute macro="PortletEntry#inputName">
			<var name="name" value="${feedTitle}" />
		</execute>

		<execute macro="PortletEntry#inputDescription">
			<var name="description" value="${description}" />
		</execute>

		<execute function="Type" locator1="TextInput#URL" value1="${targetFriendlyUrl}" />

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Web Content Constraints" />
		</execute>

		<execute function="AssertClick" locator1="WCEditWebContent#SELECT_STRUCTURE" value1="Select" />

		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="IFrame#DIALOG" />

		<execute function="AssertClickNoError" locator1="DDMSelectStructure#DDM_STRUCTURE_TABLE_NAME_LINK" value1="${structureName}">
			<var name="key_ddlDataDefinitionName" value="${structureName}" />
		</execute>

		<execute function="Confirm#assertConfirmationNoWait" value1="Selecting a new structure changes the available templates and available feed item content. Do you want to proceed?" />

		<execute function="SelectFrame" value1="relative=top" />

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Presentation Settings" />
		</execute>

		<execute function="Select" locator1="Select#FEED_ITEM_CONTENT" value1="${feedContent}" />
		<execute function="Select" locator1="Select#FEED_TYPE" value1="${feedType}" />

		<execute macro="PortletEntry#save" />
	</command>

	<command name="addLocalizedPGViaWCD">
		<execute macro="WebContentNavigator#gotoAddPGViaWCD">
			<var name="structureName" value="${localizedBasicWebContent}" />
		</execute>

		<execute macro="WebContent#addCP">
			<var name="webContentContent" value="${webContentContent}" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute function="Click" locator1="Button#PUBLISH_LOCALIZED" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addMultipleArticlesCP">
		<var method="MathUtil#sum('${articleCount}', '1')" name="articleCount" />
		<var name="count" value="1" />

		<while>
			<not>
				<equals arg1="${count}" arg2="${articleCount}" />
			</not>
			<then>
				<execute macro="WebContent#addArticleCP">
					<var name="webContentContent" value="WC Content ${count}" />
					<var name="webContentTitle" value="WC Title ${count}" />
				</execute>

				<var method="MathUtil#sum('${count}', '1')" name="count" />
			</then>
		</while>
	</command>

	<command name="addNullCP">
		<if>
			<isset var="webContentContent" />
			<then>
				<execute macro="AlloyEditor#addContent">
					<var name="content" value="${webContentContent}" />
				</execute>
			</then>
		</if>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="AssertTextEquals" locator1="Message#ERROR_FIELD_REQUIRED" value1="This field is required." />

		<if>
			<isset var="webContentContent" />
			<then>
				<execute function="AssertTextEquals" locator1="AlloyEditor#EDITOR" value1="${webContentContent}">
					<var name="key_editor" value="content" />
				</execute>
			</then>
		</if>
	</command>

	<command name="addPGViaAP">
		<execute macro="AssetPublisherPortlet#addAsset">
			<var name="assetType" value="Basic Web Content" />
		</execute>

		<execute macro="WebContent#addCP">
			<var name="webContentContent" value="${webContentContent}" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute function="AssertClickNoError" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="Pause" locator1="5000" />

		<execute function="SelectFrame" value1="relative=top" />

		<var name="key_assetTitle" value="${webContentTitle}" />

		<execute function="AssertTextEquals" locator1="AP#ASSET_ABSTRACT_TITLE" value1="${webContentTitle}" />
		<execute function="AssertTextEquals#assertPartialText" locator1="AP#ASSET_ABSTRACT_DESCRIPTION" value1="${webContentContent}" />
	</command>

	<command name="addPGViaWCD">
		<execute macro="WebContentNavigator#gotoAddPGViaWCD" />

		<execute macro="WebContent#addCP">
			<var name="webContentContent" value="${webContentContent}" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute macro="Button#clickPublish" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addPriority">
		<execute macro="Panel#expandPanel">
			<var name="panel" value="Metadata" />
		</execute>

		<execute function="Type" locator1="WC#PRIORITY" value1="${priority}" />
	</command>

	<command name="addStructuredPGViaWCD">
		<execute macro="WebContentNavigator#gotoAddPGViaWCD">
			<var name="structureName" value="${structureName}" />
		</execute>

		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="${webContentTitle}" />
		</execute>

		<if>
			<isset var="webContentText" />
			<then>
				<execute function="Type" locator1="DDMField#TEXT" value1="${webContentText}">
					<var name="key_fieldName" value="${structureFieldName}" />
				</execute>
			</then>
		</if>

		<execute function="Click" locator1="Button#PUBLISH" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addTagPGViaWCD">
		<execute macro="Panel#expandPanel">
			<var name="panel" value="Metadata" />
		</execute>

		<execute function="Type" locator1="AssetCategorization#TAGS_FIELD" value1="${tagName}" />

		<execute function="AssertClick" locator1="AssetCategorization#TAGS_ADD_BUTTON" value1="Add" />

		<execute function="AssertTextEquals" locator1="AssetCategorization#TAG" value1="${tagName}">
			<var name="key_tagName" value="${tagName}" />
		</execute>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addViaCKEditor">
		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="${webContentTitle}" />
		</execute>

		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD" value1="${webContentContent}" />
	</command>

	<command name="addWithCategoryCP">
		<execute macro="WebContent#addCP">
			<var name="webContentContent" value="${webContentContent}" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute macro="WebContent#selectCategory">
			<var name="categoryName" value="${categoryName}" />
			<var name="userSegmentDetailTitle" value="${userSegmentDetailTitle}" />
			<var name="userSegmentSelectChildSite" value="${userSegmentSelectChildSite}" />
			<var name="vocabularyName" value="${vocabularyName}" />
		</execute>

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addWithDocumentCP">
		<execute macro="WebContent#addCP">
			<var name="webContentContent" value="${webContentContent}" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute macro="ItemSelector#gotoItemSelectorViaAlloyEditor" />

		<execute function="SelectFrame" locator1="IFrame#METADATA_SETS_IFRAME" />

		<var name="key_imageFileName" value="${dmDocumentTitle}" />

		<var method="StringUtil#replace('${dmDocumentTitle}', ' ', '+')" name="key_imageFileName" />

		<execute function="Pause" locator1="1000" />

		<execute function="Click" locator1="ItemSelector#SELECT_FILE_IMAGE_PREVIEW" />

		<execute function="SelectFrameTop" value1="relative=top" />

		<execute function="AssertClick" locator1="ItemSelector#ADD_BUTTON" value1="Add" />

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addWithGlobalStructurePGViaWCD">
		<execute macro="WebContentNavigator#gotoAddPGViaWCD">
			<var name="structureName" value="${selectStructureName}" />
		</execute>

		<execute macro="WebContent#addCP">
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Structure and Template" />
		</execute>

		<execute function="AssertClick" locator1="WCEditWebContent#SELECT_STRUCTURE" value1="Select" />

		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="IFrame#DIALOG" />

		<execute function="AssertClickNoError" locator1="DDMSelectStructure#DDM_STRUCTURE_TABLE_NAME_LINK" value1="${structureName}">
			<var name="key_ddlDataDefinitionName" value="${structureName}" />
		</execute>

		<execute function="Confirm#assertConfirmationNoWait" value1="Selecting a new structure deletes all unsaved content. Do you want to proceed?" />

		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="IFrame#EDIT_ASSET" />

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Structure and Template" />
		</execute>

		<execute function="AssertTextEquals" locator1="WCEditWebContent#SELECTED_STRUCTURE" value1="${structureName}" />
		<execute function="AssertTextEquals" locator1="WCEditWebContent#SELECTED_TEMPLATE" value1="${templateName}" />

		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="${webContentTitle}" />
		</execute>

		<execute function="Type" locator1="DDMField#TEXT" value1="${webContentContent}">
			<var name="key_fieldName" value="${fieldName}" />
		</execute>

		<execute function="Click" locator1="Button#PUBLISH" />

		<execute function="SelectFrame" value1="relative=top" />

		<if>
			<equals arg1="${webContentJavascript}" arg2="true" />
			<then>
				<execute function="AssertTextEquals" locator1="//div[@class='journal-content-article']//span[contains(@class,'portlet-title-text')]" value1="${embeddedPortletTitle}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="WCD#WEB_CONTENT_CONTENT" value1="${webContentContent}" />
			</else>
		</if>
	</command>

	<command name="addWithMultipleCategories">
		<execute macro="WebContent#addCP">
			<var name="webContentContent" value="${webContentContent}" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Metadata" />
		</execute>

		<execute macro="AssetCategorization#addMultipleCategories">
			<var name="categoryNameList" value="${categoryNameList}" />
			<var name="vocabularyName" value="${vocabularyName}" />
		</execute>

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addWithStructureAndWorkflowCP">
		<execute macro="WebContent#addCP">
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<if>
			<isset var="webContentText" />
			<then>
				<execute function="Type" locator1="DDMField#TEXT" value1="${webContentText}">
					<var name="key_fieldName" value="Text" />
				</execute>
			</then>
		</if>

		<execute function="AssertClick" locator1="Button#SUBMIT_FOR_PUBLICATION" value1="Submit for Publication" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="addWithStructureCP">
		<execute function="Click" locator1="TextInput#TITLE" />

		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="${webContentTitle}" />
		</execute>

		<if>
			<isset var="webContentBoolean" />
			<then>
				<execute function="Check" locator1="DDMField#BOOLEAN" />
			</then>
		</if>

		<if>
			<isset var="webContentColor" />
			<then>
				<execute function="Click" locator1="DDMField#TEXT">
					<var name="key_fieldName" value="Color" />
				</execute>

				<execute function="Click" locator1="DDMField#COLOR_PICKER_COLOR">
					<var name="key_colorCode" value="${webContentColor}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="webContentDate" />
			<then>
				<execute function="Type" locator1="DDMField#DATE" value1="${webContentDate}" />
			</then>
		</if>

		<if>
			<isset var="webContentDecimal" />
			<then>
				<execute function="Type" locator1="DDMField#DECIMAL" value1="${webContentDecimal}">
					<var name="key_fieldName" value="Decimal" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="webContentDocumentsAndMedia" />
			<then>
				<execute function="AssertClick" locator1="Button#DOCUMENTS_AND_MEDIA" value1="${webContentDocumentsAndMedia}" />
			</then>
		</if>

		<if>
			<isset var="webContentHTML" />
			<then>
				<execute function="Type#typeEditor" locator1="DDMField#HTML" value1="${webContentHTML}">
					<var name="key_fieldName" value="HTML" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="webContentImage" />
			<then>
				<execute function="Click" locator1="Button#SELECT" />

				<execute macro="ItemSelector#uploadFile">
					<var name="navTab" value="Web Content Images" />
					<var name="uploadFileName" value="${webContentImage}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="webContentInteger" />
			<then>
				<execute function="Type" locator1="DDMField#INTEGER" value1="${webContentInteger}">
					<var name="key_fieldName" value="Integer" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="linkedPageName" />
			<then>
				<execute function="Click" locator1="WC#LINK_TO_PAGE_SELECT_PAGE_BUTTON" />

				<execute function="Click" locator1="WC#LINK_TO_PAGE_SELECT_PAGE_RADIO">
					<var name="key_pageName" value="${linkedPageName}" />
				</execute>

				<execute function="ClickNoError" locator1="Button#SELECT" />
			</then>
		</if>

		<if>
			<isset var="webContentNumber" />
			<then>
				<execute function="Type" locator1="DDMField#NUMBER" value1="${webContentNumber}">
					<var name="key_fieldName" value="Number" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="webContentRadio" />
			<then>
				<execute function="Select" locator1="DDMField#RADIO" value1="${webContentRadio}" />
			</then>
		</if>

		<if>
			<isset var="webContentSelect" />
			<then>
				<execute function="Select" locator1="DDMField#SELECT" value1="${webContentSelect}" />
			</then>
		</if>

		<if>
			<isset var="webContentSelectWCFile" />
			<then>
				<execute function="AssertClick" locator1="DDLEditRecord#WEB_CONTENT_CHOOSE_FROM_WEB_CONTENT_LIBRARY_BUTTON" value1="Select">
					<var name="key_fieldName" value="${fieldName}" />
				</execute>

				<execute function="SelectFrame" locator1="IFrame#DIALOG" />

				<execute function="AssertClick" locator1="Link#WEB_CONTENT_ARTICLE" value1="${webContentSelectWCFile}">
					<var name="key_webContentTitle" value="${webContentSelectWCFile}" />
				</execute>

				<execute function="SelectFrame" value1="relative=top" />
			</then>
		</if>

		<if>
			<isset var="webContentText" />
			<then>
				<execute function="Type" locator1="DDMField#TEXT" value1="${webContentText}">
					<var name="key_fieldName" value="Text" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="webContentTextBox" />
			<then>
				<if>
					<isset var="fieldName" />
					<then>
						<execute function="Type" locator1="DDMField#TEXT_BOX" value1="${webContentTextBox}">
							<var name="key_fieldName" value="${fieldName}" />
						</execute>
					</then>
					<else>
						<execute function="Type" locator1="DDMField#TEXT_BOX" value1="${webContentTextBox}">
							<var name="key_fieldName" value="TextBox" />
						</execute>
					</else>
				</if>
			</then>
		</if>

		<if>
			<isset var="webContentTextBox2" />
			<then>
				<execute function="Type" locator1="DDMField#TEXT_BOX" value1="${webContentTextBox2}">
					<var name="key_fieldName" value="${fieldName2}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="webContentTextBoxList" />
			<then>
				<var name="count" value="1" />

				<for list="${webContentTextBoxList}" param="webContentTextBox">
					<execute function="Type" locator1="DDMField#TEXT_BOX" value1="${webContentTextBox}">
						<var name="key_fieldName" value="heading${count}" />
					</execute>

					<var method="MathUtil#sum('${count}', '1')" name="count" />
				</for>
			</then>
		</if>

		<if>
			<isset var="description" />
			<then>
				<execute macro="AlloyEditor#addDescription">
					<var name="description" value="${description}" />
				</execute>
			</then>
		</if>

		<if>
			<equals arg1="${saveAsDraft}" arg2="true" />
			<then>
				<execute macro="PortletEntry#saveAsDraft" />

				<execute function="AssertTextEquals" locator1="AssetWorkflow#WORKFLOW_STATUS" value1="Draft" />
			</then>
		</if>
	</command>

	<command name="addWithTagCP">
		<execute macro="WebContent#addCP">
			<var name="webContentContent" value="${webContentContent}" />
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panel" value="Metadata" />
		</execute>

		<for list="${tagNameList}" param="tagName">
			<execute function="Click" locator1="AssetCategorization#TAGS_ADD_BUTTON" />

			<execute function="Type#clickAtType" locator1="AssetCategorization#TAGS_FIELD" value1="${tagName}" />
		</for>

		<execute function="Click" locator1="AssetCategorization#TAGS_ADD_BUTTON" />

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addWithThreeTranslations">
		<execute macro="WebContent#addCP">
			<var name="webContentContent" value="WC Content" />
			<var name="webContentTitle" value="WC Title" />
		</execute>

		<execute macro="PortletEntry#changeLocale">
			<var name="fieldName" value="Title" />
			<var name="locale" value="es-ES" />
		</execute>

		<execute macro="WebContent#addCP">
