	}

	macro addTranslation {
		AssertTextEquals(
			locator1 = "TextInput#TITLE",
			value1 = "${webContentTitle}");

		Click(locator1 = "Translation#ADD");

		MenuItem.click(menuItem = "${localizedLanguage}");

		if (!(IsElementPresent(locator1 = "Translation#SELECTED_LOCALE", value1 = "${localizedLanguage}", key_localizedLanguage = "${localizedLanguage}"))) {
			Click(
				key_localizedLanguage = "${localizedLanguage}",
				locator1 = "Translation#CURRENT_LOCALE");
		}

		if (isSet(localizedTitle)) {
			PortletEntry.inputTitle(title = "${localizedTitle}");
		}

		if (isSet(localizedAbstractSummary)) {
			Type(
				locator1 = "TextArea#DESCRIPTION",
				value1 = "${localizedAbstractSummary}");
		}

		if (isSet(localizedContent)) {
			AlloyEditor.addContent(content = "${localizedContent}");
		}

		if (isSet(localizedInteger)) {
			Type(
				key_fieldName = "Integer",
				locator1 = "TextInput#DDM",
				value1 = "${localizedInteger}");
		}

		if (isSet(localizedNumber)) {
			Type(
				key_fieldName = "Number",
				locator1 = "TextInput#DDM",
				value1 = "${localizedNumber}");
		}

		if (isSet(localizedText)) {
			Type(
				key_fieldName = "Text",
				locator1 = "TextInput#DDM",
				value1 = "${localizedText}");
		}

		if (isSet(localizedTextBox)) {
			Type(
				key_fieldName = "TextBox",
				locator1 = "DDMField#TEXT_BOX",
				value1 = "${localizedTextBox}");
		}

		if (isSet(unlocalizableHTML)) {
			AssertElementNotPresent(
				key_fieldName = "${unlocalizableHTML}",
				locator1 = "DDMField#HTML");
		}

		if (isSet(unlocalizableText)) {
			AssertElementNotPresent(
				key_fieldName = "${unlocalizableText}",
				locator1 = "TextInput#DDM");
		}

		if (isSet(unlocalizableTextBox)) {
			AssertElementNotPresent(
				key_fieldName = "${unlocalizableTextBox}",
				locator1 = "DDMField#TEXT_BOX");
		}

		if (isSet(unlocalizableField)) {
			AssertTextNotPresent(
				key_fieldName = "${unlocalizableField}",
				locator1 = "DDMField#${unlocalizableFieldLocator}",
				value1 = "${unlocalizableField}");
		}
	}

	macro addViaCKEditor {
		PortletEntry.inputTitle(title = "${webContentTitle}");

		Type.typeCKEditorWaitForCKEditor(
			locator1 = "CKEditor#BODY_FIELD",
			value1 = "${webContentContent}");
	}

	macro addWithCategoryCP {
		WebContent.addCP(
			webContentContent = "${webContentContent}",
			webContentTitle = "${webContentTitle}");

		WebContent.selectCategory(
			categoryName = "${categoryName}",
			userSegmentDetailTitle = "${userSegmentDetailTitle}",
			userSegmentSelectChildSite = "${userSegmentSelectChildSite}",
			vocabularyName = "${vocabularyName}");

		PortletEntry.publish();
	}

	macro addWithDocumentCP {
		WebContent.addCP(
			webContentContent = "${webContentContent}",
			webContentTitle = "${webContentTitle}");

		ItemSelector.gotoItemSelectorViaAlloyEditor();

		SelectFrame(locator1 = "IFrame#DIALOG");

		var key_imageFileName = "${dmDocumentTitle}";
		var key_imageFileName = StringUtil.replace("${dmDocumentTitle}", " ", "+");

		Pause(locator1 = "5000");

		if (isSet(siteName)) {
			Click(locator1 = "ItemSelector#SITES_NAVIGATION");

			AssertClick(
				key_siteName = "${siteName}",
				locator1 = "ItemSelector#SITE_SELECTOR",
				value1 = "${siteName}");
		}

		if (!(IsElementPresent(locator1 = "ItemSelector#SELECT_FILE_IMAGE_PREVIEW"))) {
			Click(locator1 = "Icon#DISPLAY_STYLE_DESCRIPTIVE");

			Click(locator1 = "Icon#DISPLAY_STYLE_ICON");

			AssertElementPresent.pauseAssertVisible(locator1 = "Icon#DISPLAY_STYLE_ICON_ACTIVE");
		}

		Click(locator1 = "ItemSelector#SELECT_FILE_IMAGE_PREVIEW");

		SelectFrameTop(value1 = "relative=top");

		AssertClick(
			locator1 = "ItemSelector#ADD_BUTTON",
			value1 = "Add");

		PortletEntry.publish();
	}

	macro addWithGlobalStructurePGViaWCD {
		WebContentNavigator.gotoAddPGViaWCD(structureName = "${selectStructureName}");

		PortletEntry.inputTitle(title = "${webContentTitle}");

		Panel.expandPanel(panelHeading = "Structure and Template");

		AssertClick(
			locator1 = "WCEditWebContent#SELECT_STRUCTURE",
			value1 = "Select");

		SelectFrame(value1 = "relative=top");

		IFrame.selectDialogFrame();

		AssertClickNoError(
			key_ddlDataDefinitionName = "${structureName}",
			locator1 = "DDMSelectStructure#DDM_STRUCTURE_TABLE_NAME_LINK",
			value1 = "${structureName}");

		Confirm.assertConfirmationNoWait(value1 = "Selecting a new structure deletes all unsaved content. Do you want to proceed?");

		SelectFrame(value1 = "relative=top");

		SelectFrame(locator1 = "IFrame#EDIT_ASSET");

		Panel.expandPanel(panelHeading = "Structure and Template");

		AssertTextEquals(
			locator1 = "WCEditWebContent#SELECTED_STRUCTURE",
			value1 = "${structureName}");

		AssertTextEquals(
			locator1 = "WCEditWebContent#SELECTED_TEMPLATE",
			value1 = "${templateName}");

		PortletEntry.inputTitle(title = "${webContentTitle}");

		Type(
			key_fieldName = "${fieldName}",
			locator1 = "TextInput#DDM",
			value1 = "${webContentContent}");

		Click(locator1 = "Button#PUBLISH");

		SelectFrame(value1 = "relative=top");

		if ("${webContentJavascript}" == "true") {
			AssertTextEquals(
				locator1 = "//div[contains(@class,'journal-content-article')]//span[contains(@class,'portlet-title-text')]",
				value1 = "${embeddedPortletTitle}");
		}
		else {
			AssertTextEquals(
				locator1 = "WCD#WEB_CONTENT_CONTENT",
				value1 = "${webContentContent}");
		}
	}

	macro addWithImage {
		ItemSelector.gotoItemSelectorViaAlloyEditor();

		SelectFrame(locator1 = "IFrame#DIALOG");

		AssertElementPresent(locator1 = "ItemSelector#SELECT_FILE_UPLOAD_IMAGE");

		UploadCommonFile.uploadCommonFileHiddenNoMouseOver(
			locator1 = "TextInput#FILE",
			value1 = "${uploadFileName}");

		SelectFrame(locator1 = "ItemSelector#ITEM_SELECTOR_MODAL_FOOTER");

		AssertClick(
			locator1 = "ItemSelector#ADD_BUTTON",
			value1 = "Add");

		SelectFrameTop(value1 = "relative=top");
	}

	macro addWithMultipleCategories {
		WebContent.addCP(
			webContentContent = "${webContentContent}",
			webContentTitle = "${webContentTitle}");

		Panel.expandPanel(panelHeading = "Metadata");

		AssetCategorization.addMultipleCategories(
			categoryNameList = "${categoryNameList}",
			vocabularyName = "${vocabularyName}");

		PortletEntry.publish();
	}

	macro addWithStructureAndWorkflowCP {
		PortletEntry.inputTitle(title = "${webContentTitle}");

		if (isSet(webContentText)) {
			Type(
				key_fieldName = "Text",
				locator1 = "TextInput#DDM",
				value1 = "${webContentText}");
		}

		AssertClick(
			locator1 = "Button#SUBMIT_FOR_PUBLICATION",
			value1 = "Submit for Publication");

		Alert.viewSuccessMessage();
	}

	macro addWithStructureCP {
		Click(locator1 = "TextInput#TITLE");

		PortletEntry.inputTitle(title = "${webContentTitle}");

		if (isSet(webContentBoolean)) {
			Check(locator1 = "DDMField#BOOLEAN");
		}

		if ((isSet(dateFieldName)) && (isSet(webContentDate))) {
			Type(
				key_fieldName = "${dateFieldName}",
				locator1 = "TextInput#DDM",
				value1 = "${webContentDate}");
		}
		else if (isSet(webContentDate)) {
			Type(
				locator1 = "TextInput#DDM",
				value1 = "${webContentDate}");
		}

		if (isSet(webContentDecimal)) {
			Type(
				key_fieldName = "Decimal",
				locator1 = "TextInput#DDM",
				value1 = "${webContentDecimal}");
		}

		if (isSet(webContentDocumentsAndMedia)) {
			AssertClick(
				key_fieldName = "${fieldName}",
				locator1 = "Button#FIELD_SELECT",
				value1 = "Select");

			SelectFrame(locator1 = "IFrame#DIALOG");

			AssertClick.assertPartialTextClickAt(
				key_dmDocumentTitle = "${webContentDocumentsAndMedia}",
				locator1 = "WC#ICON_DOCUMENT_TITLE",
				value1 = "${webContentDocumentsAndMedia}");

			SelectFrameTop(value1 = "relative=top");

			Click(locator1 = "Button#ADD");
		}

		if (isSet(webContentHTML)) {
			Type.typeEditor(
				key_fieldName = "HTML",
				locator1 = "DDMField#HTML",
				value1 = "${webContentHTML}");
		}

		if (isSet(webContentImage)) {
			Click(locator1 = "Button#SELECT");

			ItemSelector.uploadFile(
				navTab = "Web Content Images",
				uploadFileName = "${webContentImage}");
		}

		if (isSet(webContentImageUnderDocumentsAndMedia)) {
			Click(locator1 = "Button#SELECT");

			ItemSelector.uploadFile(
				navTab = "Documents and Media",
				uploadFileName = "${webContentImageUnderDocumentsAndMedia}");
		}

		if (isSet(webContentInteger)) {
			Type(
				key_fieldName = "Integer",
				locator1 = "TextInput#DDM",
				value1 = "${webContentInteger}");
		}

		if (isSet(linkedPageName)) {
			var key_pageName = "${linkedPageName}";

			AssertClick(
				key_fieldName = "${linkFieldName}",
				locator1 = "Button#FIELD_SELECT",
				value1 = "Select");

			while (IsElementNotPresent(locator1 = "WC#LINK_TO_PAGE_SELECT_PAGE_RADIO")) {
				Click(locator1 = "WC#LINK_TO_PAGE_NEXT_PAGE");
			}

			Click(locator1 = "WC#LINK_TO_PAGE_SELECT_PAGE_RADIO");

			ClickNoError(locator1 = "Button#SELECT");
		}

		if (isSet(webContentNumber)) {
			Type(
				key_fieldName = "Number",
				locator1 = "TextInput#DDM",
				value1 = "${webContentNumber}");
		}

		if (isSet(webContentRadio)) {
			Select(
				locator1 = "DDMField#RADIO",
				value1 = "${webContentRadio}");
		}

		if (isSet(webContentSelect)) {
			Select(
				locator1 = "DDMField#SELECT",
				value1 = "${webContentSelect}");
		}

		if (isSet(webContentSelectWCFile)) {
			AssertClick(
				key_fieldName = "${fieldName}",
				locator1 = "Button#FIELD_SELECT",
				value1 = "Select");

			IFrame.selectDialogFrame();

			AssertClick(
				key_webContentTitle = "${webContentSelectWCFile}",
				locator1 = "Link#WEB_CONTENT_ARTICLE",
				value1 = "${webContentSelectWCFile}");

			SelectFrame(value1 = "relative=top");
		}

		if ((isSet(webContentText)) && (isSet(textFieldName))) {
			Type(
				key_fieldName = "${textFieldName}",
				locator1 = "TextInput#DDM",
				value1 = "${webContentText}");
		}
		else if (isSet(webContentText)) {
			Type(
				key_fieldName = "Text",
				locator1 = "TextInput#DDM",
				value1 = "${webContentText}");
		}

		if (isSet(webContentTextBox)) {
			if (isSet(fieldName)) {
				Type(
					key_fieldName = "${fieldName}",
					locator1 = "DDMField#TEXT_BOX",
					value1 = "${webContentTextBox}");
			}
			else {
				Type(
					key_fieldName = "TextBox",
					locator1 = "DDMField#TEXT_BOX",
					value1 = "${webContentTextBox}");
			}
		}

		if (isSet(webContentTextBox2)) {
			Type(
				key_fieldName = "${fieldName2}",
				locator1 = "DDMField#TEXT_BOX",
				value1 = "${webContentTextBox2}");
		}

		if (isSet(webContentTextBoxList)) {
			var count = "1";

			for (var webContentTextBox : list "${webContentTextBoxList}") {
				Type(
					key_fieldName = "heading${count}",
					locator1 = "DDMField#TEXT_BOX",
					value1 = "${webContentTextBox}");

				var count = ${count} + 1;
			}
		}

		if (isSet(abstractSummary)) {
			Type(
				locator1 = "TextArea#DESCRIPTION",
				value1 = "${abstractSummary}");
		}

		if (isSet(pageName)) {
			Panel.expandPanel(panelHeading = "Display Page");

			AssertClick(
				locator1 = "Button#CHOOSE",
				value1 = "Choose");

			Pause(locator1 = "5000");

			SelectFrame(locator1 = "WCSelectDisplayPage#DISPLAY_PAGE_CONFIGURATION_IFRAME");

			AssertClick(
				key_pageName = "${pageName}",
				locator1 = "WCSelectDisplayPage#PUBLIC_PAGES_SELECTION",
				value1 = "${pageName}");

			SelectFrame(value1 = "relative=top");

			Button.clickDone();

			SelectFrame(value1 = "relative=top");

			AssertTextEquals(
				locator1 = "WCEditWebContent#DISPLAY_PAGE_SELECTION",
				value1 = "Public Pages > ${pageName}");
		}

		if ("${saveAsDraft}" == "true") {
			Button.clickSaveAsDraft();

			Alert.viewSuccessMessage();

			AssertTextEquals(
				locator1 = "AssetWorkflow#WORKFLOW_STATUS",
				value1 = "Draft");
		}
	}

	macro addWithTagCP {
		WebContent.addCP(
			webContentContent = "${webContentContent}",
			webContentTitle = "${webContentTitle}");

		Panel.expandPanel(panelHeading = "Metadata");

		if ("${selectTag}" == "true") {
			AssetCategorization.selectTag(tagNameList = "${tagNameList}");
		}
		else {
			for (var tagName : list "${tagNameList}") {
