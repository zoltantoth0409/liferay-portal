	macro addPriority {
		Panel.expandPanel(panelHeading = "Metadata");

		Type(locator1 = "WC#PRIORITY", value1 = "${priority}");
	}

	macro addSmallImageViaURL {
		Panel.expandPanel(panelHeading = "Small Image");

		Check(locator1 = "WCEditWebContent#USE_SMALL_IMAGE_CHECKBOX");
		Click(locator1 = "WCEditWebContent#SMALL_IMAGE_URL");

