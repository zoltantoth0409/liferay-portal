	}

	macro addTagPGViaWCD {
		Panel.expandPanel(panelHeading = "Metadata");

		Type(
			locator1 = "AssetCategorization#TAGS_FIELD",
			value1 = "${tagName}");

		AssertClick(
			locator1 = "Button#ADD_TAGS",
			value1 = "Add");

		AssertTextEquals(
			key_tagName = "${tagName}",
			locator1 = "AssetCategorization#TAG",
