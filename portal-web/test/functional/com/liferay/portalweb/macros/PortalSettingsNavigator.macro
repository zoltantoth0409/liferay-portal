definition {

	macro expandAuthenticationPanel {
		Panel.expandPanel(panel = "Authentication");
	}

	macro gotoCAS {
		PortalSettingsNavigator.expandAuthenticationPanel();

		AssertClick(
			locator1 = "NavTab#CAS",
			value1 = "CAS");
	}

