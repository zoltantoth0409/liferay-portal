	macro _signInLocalization {
		if ("${localization}" == "JA") {
			var localizedSignIn = "ログイン";
		}
		else if ("${localization}" == "ZH") {
			var localizedSignIn = "登录";
		}
		else if ("${localization}" == "ES") {
			var localizedSignIn = "Acceder";
		}
		else {
			var localizedSignIn = "Sign In";
		}

		return "${localizedSignIn}";
	}

