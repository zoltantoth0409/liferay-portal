	macro viewIdentificationInformation {
		Organization.gotoIdentificationCP();

		// Address

		if (isSet(city)) {
			AssertTextEquals(
				locator1 = "TextInput#CITY",
				value1 = "${city}");
		}

		if (isSet(country)) {
			AssertSelectedLabel(
				locator1 = "Select#COUNTRY",
				value1 = "${country}");
		}

		if (isSet(postalCode)) {
			AssertTextEquals(
				locator1 = "TextInput#POSTAL_CODE",
				value1 = "${postalCode}");
		}

		if (isSet(region)) {
			AssertSelectedLabel(
				locator1 = "Select#REGION",
				value1 = "${region}");
		}

		if (isSet(street1)) {
			AssertTextEquals(
				locator1 = "TextInput#STREET_1",
				value1 = "${street1}");
		}

		if (isSet(addressType)) {
			AssertSelectedLabel(
				locator1 = "Select#TYPE",
				value1 = "${addressType}");
		}

		// Phone Numbers

		if (isSet(number)) {
			Panel.expandPanel(panelHeading = "Phone Numbers");

			AssertTextEquals.assertValue(
				locator1 = "UsersAndOrganizationsEditUser#USER_IDENTIFICATION_PHONE_NUMBERS",
				value1 = "${number}");

			if (isSet(numberType)) {
				AssertSelectedLabel(
					locator1 = "UsersAndOrganizationsEditUser#USER_IDENTIFICATION_PHONE_NUMBERS_TYPE",
					value1 = "${numberType}");
			}
		}

		// Additional Email Addresses

		if (isSet(emailAddress)) {
			Panel.expandPanel(panelHeading = "Additional Email Addresses");

			AssertTextEquals.assertValue(
				locator1 = "UsersAndOrganizationsEditUser#USER_IDENTIFICATION_ADDITIONAL_EMAIL_ADDRESSES",
				value1 = "${emailAddress}");
		}

		if (isSet(emailAddressCount)) {
			Panel.expandPanel(panelHeading = "Additional Email Addresses");

			var i = "0";

			while ("${i}" != "${emailAddressCount}") {
				var i = ${i} + 1;

				var key_emailAddress = "${emailAddressPrefix}${i}@liferay.com";

				AssertTextEquals.assertValue(
					locator1 = "UsersAndOrganizationsEditUser#USER_IDENTIFICATION_ADDITIONAL_EMAIL_ADDRESSES",
					value1 = "${emailAddressPrefix}${i}@liferay.com");
			}
		}

		// Websites

		if (isSet(url)) {
			Panel.expandPanel(panelHeading = "Websites");

			AssertTextEquals.assertValue(
				locator1 = "UsersAndOrganizationsEditUser#USER_IDENTIFICATION_WEBSITES_URL",
				value1 = "${url}");

			if (isSet(websiteType)) {
				AssertSelectedLabel(
					locator1 = "UsersAndOrganizationsEditUser#USER_IDENTIFICATION_WEBSITES_TYPE",
					value1 = "${websiteType}");
			}
		}

		// Instant Messenger

		if (isSet(skype)) {
			Panel.expandPanel(panelHeading = "Instant Messenger");

			AssertTextEquals.assertValue(
				locator1 = "TextInput#SKYPE",
				value1 = "${skype}");
		}
	}

