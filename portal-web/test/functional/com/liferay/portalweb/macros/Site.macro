	macro addBlankCP {
		Site.addCP(
			membershipType = "${membershipType}",
			parentSiteName = "${parentSiteName}",
			singleSiteTypePermission = "${singleSiteTypePermission}",
			siteName = "${siteName}",
			siteTemplateName = "${siteTemplateName}",
