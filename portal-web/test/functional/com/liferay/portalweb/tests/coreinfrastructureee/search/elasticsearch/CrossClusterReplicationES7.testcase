		property test.name.skip.portal.instance = "CrossClusterReplicationES7#SearchWithResultRankings";

		CrossClusterReplication.setupCrossClusterReplication();

		JSONWebcontent.addWebContent(
			content = "",
			groupName = "Guest",
			title = "WC Title 1");

		JSONWebcontent.addWebContent(
			content = "",
			groupName = "Guest",
			title = "WC Title 2");

		JSONWebcontent.addWebContent(
			content = "",
			groupName = "Guest",
