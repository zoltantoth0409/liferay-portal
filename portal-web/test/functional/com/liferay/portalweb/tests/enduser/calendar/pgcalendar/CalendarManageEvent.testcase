		CalendarScheduler.viewEventWithReminders(eventTitle = "Calendar Event Regular Title Edit");

		CalendarEvent.view(
			eventTitle = "Calendar Event Regular Title Edit",
			frequency = "Daily",
			numberOfOccurrences = "1");
