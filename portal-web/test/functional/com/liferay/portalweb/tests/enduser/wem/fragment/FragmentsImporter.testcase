			key_position = "1",
			locator1 = "PageEditor#GRID_COLUMN_NESTED_ELEMENT");

		PageEditor.clickEditableField(
			contributed = "true",
			fragmentName = "HTML",
			id = "element-html");

		AssertElementPresent(
			key_columnNumber = "3",
			key_elementHeader = "//div[contains(@class,'page-editor')]//h1[contains(.,'HTML Example')]",
			key_position = "1",
