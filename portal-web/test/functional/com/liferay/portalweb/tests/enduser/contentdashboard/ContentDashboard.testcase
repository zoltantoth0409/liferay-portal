		Pause(locator1 = "5000");

		ContentDashboard.filterByAuthorTable(filterOption = "Author");

		Pause(locator1 = "3000");

		AssertTextPresent(
			locator1 = "ContentDashboard#FIRST_ELEMENT_LISTED",
