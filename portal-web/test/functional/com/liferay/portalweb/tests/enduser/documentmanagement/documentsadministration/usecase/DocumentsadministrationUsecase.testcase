	@priority = "5"
	test CheckoutAndCheckin {
		ProductMenu.gotoPortlet(
			category = "Users",
			panel = "Control Panel",
			portlet = "Users and Organizations");

		User.addCP(
			userEmailAddress = "userea@liferay.com",
			userFirstName = "userfn",
			userLastName = "userln",
			userScreenName = "usersn");

		ProductMenu.gotoPortlet(
			category = "Users",
			panel = "Control Panel",
			portlet = "Users and Organizations");

		User.editPasswordCP(
			userEmailAddress = "userea@liferay.com",
			userScreenName = "usersn");

		ProductMenu.gotoPortlet(
			category = "Users",
			panel = "Control Panel",
			portlet = "Roles");

		Role.assignRegRoleCP(
			roleName = "Administrator",
			userScreenName = "usersn");

		Navigator.openURL();

		ProductMenu.gotoPortlet(
			category = "Members",
			panel = "Site Administration",
			portlet = "Site Memberships");

		Site.assignUserAsMemberCP(
			userFirstName = "userfn",
			userLastName = "userln",
			userScreenName = "usersn");

		Page.add(pageName = "Documents and Media Page");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		Portlet.addPG(
			pageName = "Documents and Media Page",
			portletName = "Documents and Media");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		Click(locator1 = "Icon#DISPLAY_STYLE_DESCRIPTIVE");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		DMDocument.addPG(
			dmDocumentDescription = "DM Document Description",
			dmDocumentFile = "Document_1.doc",
			dmDocumentTitle = "Document_1.doc");

		User.logoutPG();

		Navigator.gotoPage(pageName = "Documents and Media Page");

		Click(locator1 = "Icon#DISPLAY_STYLE_DESCRIPTIVE");

		DMDocument.viewDocumentToolbarCmdAsGuestPG(dmDocumentTitle = "Document_1.doc");

		User.loginUserPG(
			password = "test",
			userEmailAddress = "userea@liferay.com");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		Click(locator1 = "Icon#DISPLAY_STYLE_DESCRIPTIVE");

		DMDocument.enableActionsMenuOnPortlet();

		DMDocument.checkoutPG(dmDocumentTitle = "Document_1.doc");

		DMDocument.viewDocumentVersionNumberCmdPG(dmDocumentVersionNumber = "PWC");

		AssertTextEquals(
			locator1 = "DocumentsAndMediaDocument#DOCUMENT_INFO_WORKFLOW_STATUS",
			value1 = "Draft");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		for (var tagCount : list "1,2") {
			DMDocument.editCmdPG(
				dmDocumentTagName = "tag${tagCount}",
				dmDocumentTitle = "Document_1.doc");

			AssertClick(
				locator1 = "Button#SAVE",
				value1 = "Save");

			Pause(locator1 = "10000");

			Navigator.gotoPage(pageName = "Documents and Media Page");
		}

		DMDocument.editSavePG(
			dmDocumentTitle = "Document_1.doc",
			dmDocumentTitleEdit = "Document1");

		User.logoutAndLoginPG(
			userLoginEmailAddress = "test@liferay.com",
			userLoginFullName = "Test Test");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		DMDocument.editCmdPG(
			checkedOut = "true",
			dmDocumentTitle = "Document_1.doc",
			userName = "userfn userln");

		User.logoutAndLoginPG(
			userLoginEmailAddress = "userea@liferay.com",
			userLoginFullName = "userfn userln");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		DMNavigator.gotoCheckedOutDocument(
			dmCheckedOutDocumentTitle = "Document1",
			dmDocumentTitle = "Document_1.doc");

		DMDocument.viewDocumentVersionNumberCmdPG(dmDocumentVersionNumber = "PWC");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		DMDocument.saveAndCheckinEditedDocumentPG(
			dmDocumentTitle = "Document1",
			revision = "Minor");

		DMNavigator.gotoDocumentPG(dmDocumentTitle = "Document1");

		DMDocument.viewDocumentVersionNumberCmdPG(dmDocumentVersionNumber = "1.1");

		AssertTextEquals(
			locator1 = "DocumentsAndMediaDocument#DOCUMENT_INFO_WORKFLOW_STATUS",
			value1 = "Approved");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		DMDocument.checkoutPG(dmDocumentTitle = "Document1");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		AssertClick(
			key_dmDocumentTitle = "Document1",
			locator1 = "DocumentsAndMedia#DESCRIPTIVE_LIST_DOCUMENT_TITLE",
			value1 = "Document1");

		AssertClick(
			locator1 = "Button#EDIT",
			value1 = "Edit");

		while (IsElementPresent(locator1 = "AssetCategorization#TAGS_REMOVE_ICON_GENERIC")) {
			Click(locator1 = "AssetCategorization#TAGS_REMOVE_ICON_GENERIC");
		}

		AssertClick(
			locator1 = "Button#SAVE",
			value1 = "Save");

		DMDocument.viewDocumentVersionNumberCmdPG(dmDocumentVersionNumber = "PWC");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		DMDocument.checkinPG(
			dmDocumentTitle = "Document1",
			revision = "Minor");

		DMDocument.viewDocumentVersionNumberCmdPG(dmDocumentVersionNumber = "1.1");

		AssertTextEquals(
			locator1 = "DocumentsAndMediaDocument#DOCUMENT_INFO_WORKFLOW_STATUS",
			value1 = "Approved");
	}

