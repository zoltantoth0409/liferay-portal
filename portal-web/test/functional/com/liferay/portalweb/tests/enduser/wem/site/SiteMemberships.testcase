	@priority = "3"
	test SiteMembershipsPagination {
		property testray.component.names = "Site Administration,Users and Organizations";

		ProductMenu.gotoPortlet(
			category = "Users",
			panel = "Control Panel",
			portlet = "Users and Organizations");

		User.addMultipleCP(usersEmailsList = "userfn1@liferay.com,userfn2@liferay.com,userfn3@liferay.com,userfn4@liferay.com,userfn5@liferay.com,userfn6@liferay.com,userfn7@liferay.com,userfn8@liferay.com,userfn9@liferay.com,userfn10@liferay.com");

		ProductMenu.gotoPortlet(
			category = "Sites",
			panel = "Control Panel",
			portlet = "Sites");

		Site.addBlankCP(siteName = "Site Name");

		ProductMenu.gotoPortlet(
			category = "People",
			panel = "Site Administration",
			portlet = "Memberships");

		Site.addAllMembersCP();

		SitePages.openPagesAdmin(siteURLKey = "Site Name");

		ProductMenu.gotoPortlet(
			category = "People",
			panel = "Site Administration",
			portlet = "Memberships");

		Site.viewMemberListCP(usersEmailsList = "userfn1@liferay.com,userfn2@liferay.com,userfn3@liferay.com,userfn4@liferay.com,userfn5@liferay.com,userfn6@liferay.com,userfn7@liferay.com,userfn8@liferay.com,userfn9@liferay.com,userfn10@liferay.com");

		Site.viewMemberCP(
			userFirstName = "Test",
			userLastName = "Test",
			userScreenName = "test");

		NavItem.gotoUsers();

		AssertClick.assertPartialTextClickAt(
			locator1 = "Pagination#ITEMS_PER_PAGE_SELECT",
			value1 = "20 Entries");

		Click(locator1 = "Pagination#FIVE_ITEMS_PER_PAGE");

		AssertTextEquals(
			locator1 = "Pagination#CURRENT_PAGE_SELECT",
			value1 = "Showing 1 to 5 of 11 entries.");

		Click(locator1 = "Pagination#NEXT_LINK");

		AssertTextEquals(
			locator1 = "Pagination#CURRENT_PAGE_SELECT",
			value1 = "Showing 6 to 10 of 11 entries.");

		Site.assertMemberListNotVisibleCP(usersEmailsList = "userfn1@liferay.com,userfn2@liferay.com,userfn3@liferay.com,userfn10@liferay.com");

		Site.viewMemberListCP(usersEmailsList = "userfn4@liferay.com,userfn5@liferay.com,userfn6@liferay.com,userfn7@liferay.com,userfn8@liferay.com");

		Site.assertMemberListNotVisibleCP(usersEmailsList = "userfn9@liferay.com");

		Click(locator1 = "Pagination#FIRST_LINK");

		AssertTextEquals(
			locator1 = "Pagination#CURRENT_PAGE_SELECT",
			value1 = "Showing 1 to 5 of 11 entries.");

		Site.viewMemberListCP(usersEmailsList = "userfn1@liferay.com,userfn2@liferay.com,userfn3@liferay.com,userfn10@liferay.com");

		Site.assertMemberListNotVisibleCP(usersEmailsList = "userfn4@liferay.com,userfn5@liferay.com,userfn6@liferay.com,userfn7@liferay.com,userfn8@liferay.com");

		Site.assertMemberListNotVisibleCP(usersEmailsList = "userfn9@liferay.com");

		Site.viewMemberCP(
			userFirstName = "Test",
			userLastName = "Test",
			userScreenName = "test");

		Click(locator1 = "Pagination#LAST_LINK");

		AssertTextEquals(
			locator1 = "Pagination#CURRENT_PAGE_SELECT",
			value1 = "Showing 11 to 11 of 11 entries.");

		Site.assertMemberListNotVisibleCP(usersEmailsList = "userfn1@liferay.com,userfn2@liferay.com,userfn3@liferay.com,userfn10@liferay.com");

		Site.assertMemberListNotVisibleCP(usersEmailsList = "userfn4@liferay.com,userfn5@liferay.com,userfn6@liferay.com,userfn7@liferay.com,userfn8@liferay.com");

		Site.viewMemberListCP(usersEmailsList = "userfn9@liferay.com");

		Click(locator1 = "Pagination#PREVIOUS_LINK");

		AssertTextEquals(
			locator1 = "Pagination#CURRENT_PAGE_SELECT",
			value1 = "Showing 6 to 10 of 11 entries.");

		Site.assertMemberListNotVisibleCP(usersEmailsList = "userfn1@liferay.com,userfn2@liferay.com,userfn3@liferay.com,userfn10@liferay.com");

		Site.viewMemberListCP(usersEmailsList = "userfn4@liferay.com,userfn5@liferay.com,userfn6@liferay.com,userfn7@liferay.com,userfn8@liferay.com");

		Site.assertMemberListNotVisibleCP(usersEmailsList = "userfn9@liferay.com");
	}

