	@description = "This is a usecase for LPS-75241"
	@priority = "5"
	test EmbedVideo {
		property portal.acceptance = "true";
		property testray.component.names = "WYSIWYG";

		Blogs.addEntry(
			embedVideo = "https://www.youtube.com/watch?v=2EPZxIC5ogU",
