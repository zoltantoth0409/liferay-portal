		AssertVisible(locator1 = "Button#PLUS");

		ProductMenu.gotoPortlet(
			category = "Publishing",
			panel = "Site Administration",
			portlet = "Staging");

		Staging.activateStagingCP(
			siteName = "Site Name",
			uncheckContent = "Application Display Templates");

		Navigator.gotoSitePage(
			pageName = "Staging Test Page",
			siteName = "Site Name");

		ProductMenu.gotoPortlet(
			category = "Build",
			panel = "Site Administration",
			portlet = "Application Display Templates");

		ApplicationDisplayTemplates.addADTWithFieldEntryCP(
			fieldName = "Asset Entry",
			portletName = "Asset Publisher");

		AssertTextEquals(
			locator1 = "ApplicationDisplayTemplates#ADT_TABLE_NAME_1",
