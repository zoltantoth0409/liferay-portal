		SitePages.openPagesAdmin(siteURLKey = "site-name-staging");

		SitePages.addPublicPage(
			collectionName = "Page Template Collection",
			pageName = "Test Page Name",
			pageTemplateName = "Page Template Name");

		SitePages.openPagesAdmin(siteURLKey = "site-name-staging");

