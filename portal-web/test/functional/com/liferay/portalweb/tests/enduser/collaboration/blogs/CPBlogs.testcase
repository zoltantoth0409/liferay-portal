		BlogsEntry.viewTitle(entryTitle = "CKEditor Entry Title");

		SelectFrame.selectFrameNoLoading(locator1 = "CKEditor#BODY_FIELD_IFRAME");

		CKEditor.viewEntryContent(content = "CKEditor Entry Content");
