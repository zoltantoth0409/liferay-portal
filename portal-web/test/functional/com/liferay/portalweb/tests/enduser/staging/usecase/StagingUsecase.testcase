	@description = "This is a use case for LPS-82757."
	@priority = "3"
	test PublishWebContentWithFeeds {
		property test.name.skip.portal.instance = "StagingUsecase#PublishWebContentWithFeeds";

		ApplicationsMenu.gotoPortlet(
