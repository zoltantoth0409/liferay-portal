		Staging.viewAddButtonDoesNotExist();

		ProductMenu.gotoPortlet(
			category = "Content",
			panel = "Site Administration",
			portlet = "Web Content"
		);

		NavItem.gotoStructures();

		Staging.viewAddButtonDoesNotExist();
