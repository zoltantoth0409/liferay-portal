			<var name="panel" value="Site Administration" />
			<var name="portlet" value="Blogs" />
		</execute>

		<execute macro="Blogs#addEntry">
			<var name="entryContent" value="This is a blog about Potatoes" />
			<var name="entryTitle" value="Potato Blog" />
		</execute>

		<execute macro="ProductMenu#gotoPortlet">
			<var name="category" value="Navigation" />
			<var name="panel" value="Site Administration" />
			<var name="portlet" value="Site Pages" />
		</execute>

		<execute macro="SitePages#addPublicPage">
			<var name="pageName" value="Search Page" />
		</execute>

		<for list="Type Facet,Search Results,Search Bar" param="portletName">
