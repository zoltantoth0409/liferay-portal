	@priority = "5"
	test OutputSmoke {

		// OutputFormatGeneral

		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/outputformat/general");

		AssertTextEquals(
			locator1 = "JSFShowcase#EXAMPLE_TEXT",
			value1 = "First: Alpha, Last: Omega");

		// OutputLabelGeneral

		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/outputlabel/general");

		JSFShowcase.validateRequired();

		JSFShowcase.submitText(
			password = "Philip",
			text = "First Name",
			textValue = "Philip");

		// OutputLinkConversion

		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/outputlink/conversion");

		Click(
			key_opt = "United States",
			locator1 = "JSFShowcase#OUTPUTLINK_SELECT");

		AssertElementPresent(
			key_attribute = "Link to a map of United States",
			locator1 = "JSFShowcase#OUTPUTLINK_VALUE");

		JSFShowcase.viewLink(
			image = "false",
			link = "United+States",
			linkURL = "maps/place");

		// OutputLinkGeneral

		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/outputlink/general");

		JSFShowcase.viewLink(
			image = "false",
			link = "liferay-projects/liferay-faces",
			linkURL = "liferay-projects/liferay-faces");

		for (var link : list "liferay-projects/liferay-faces,liferay+faces") {
			JSFShowcase.viewLink(
				link = "${link}",
				linkURL = "${link}");
		}

		// OutputScriptResource

		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/outputscript/resource");

		AssertTextEquals.assertPartialText(
			locator1 = "JSFShowcase#EXAMPLE_TEXT",
			value1 = "A script");

		// OutputScriptGeneral

		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/outputscript/general");

		AssertTextEquals.assertPartialText(
			locator1 = "JSFShowcase#EXAMPLE_TEXT",
			value1 = "is rendered inline");

		// OutputStylesheetGeneral

		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/outputstylesheet/general");

		JSFShowcase.viewStylesheet();

		// OutputTextConversionTester

		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/outputtext/conversion");

		var displayDate = DateUtil.getFormattedCurrentDate("EEEE, MMM dd, yyyy");

		AssertTextEquals(
			locator1 = "JSFShowcase#EXAMPLE_TEXT",
			value1 = "${displayDate}");

		// OutputTextConversionTester

		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/outputtext/general");

		AssertTextEquals(
			locator1 = "JSFShowcase#EXAMPLE_TEXT",
			value1 = "This is text from a model bean property.");

		// OutputTextRightToLeftlTester

		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/outputtext/right-to-left");

		AssertTextEquals(
			locator1 = "JSFShowcase#EXAMPLE_TEXT_3",
			value1 = "בְּרֵאשִׁית בָּרָא אֱלֹהִים אֵת הַשָּׁמַיִם וְאֵת הָאָֽרֶץ");
	}

	@priority = "5"
	test PanelSmoke {
