		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/inputtextarea/validation");

		Type(
			locator1 = "JSFShowcase#CONVERSION_AREA_1",
			value1 = "HelloWorldcom");

