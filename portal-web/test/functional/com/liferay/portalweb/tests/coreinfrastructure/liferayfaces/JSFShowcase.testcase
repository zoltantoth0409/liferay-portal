	@priority = "5"
	test MessageSmoke {
		Navigator.openWithAppendToBaseURL(urlAppend = "web/guest/jsf-showcase/-/jsf-tag/h/message/general");

		JSFShowcase.submitMessage(
			message = "You typed 5 characters.",
