		BridgeIssues.addPageAndPortlet(
			pageURL = "mojarra-4345-workaround",
			portletName = "MOJARRA-4345-workaround");

		Navigator.openWithAppendToBaseURL(urlAppend = "web/bridge-issues/mojarra-4345-workaround");

