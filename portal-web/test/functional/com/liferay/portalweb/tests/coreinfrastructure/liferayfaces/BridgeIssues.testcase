		BridgeIssues.addPageAndPortlet(
			pageURL = "faces-3300",
			portletName = "FACES-3300");

		Navigator.openWithAppendToBaseURL(urlAppend = "web/bridge-issues/faces-3300");

		for (var resourceActionIndex : list "1,2,3,4,5,6") {
