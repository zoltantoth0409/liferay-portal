		BridgeIssues.addPageAndPortlet(
			pageURL = "mojarra-4340-workaround",
			portletName = "MOJARRA-4340-workaround");

		Navigator.openWithAppendToBaseURL(urlAppend = "web/bridge-issues/mojarra-4340-workaround");

