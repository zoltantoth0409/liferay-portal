<profile-definition>
	<name>Liferay @lp.version@</name>
	<description>This is the Liferay @lp.version@ Portal server profile for use with Tcat Server.</description>
	<environmentVariables>
		<entry>
			<string>JAVA_OPTS</string>
			<string>${JAVA_OPTS} -Dfile.encoding=UTF8 -Djava.locale.providers=JRE,COMPAT,CLDR -Djava.net.preferIPv4Stack=true -Duser.timezone=GMT -Xms2560m -Xmx2560m -XX:MaxNewSize=1536m -XX:MaxMetaspaceSize=768m -XX:MetaspaceSize=768m -XX:NewSize=1536m -XX:SurvivorRatio=7</string>
		</entry>
	</environmentVariables>
	<artifactToVersion></artifactToVersion>
</profile-definition>